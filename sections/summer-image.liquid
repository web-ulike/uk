<style>
.comparison-image{
  position: relative;
}

.comparison-image .img{
  width: 100%;
  position: relative;
  z-index: 1;
}

.comparison-image .mb-box{
  display: none;
}

.comparison-image .comparison-image-cont{
  position: absolute;
  top: 34.1%;
  right: 7.5%;
  max-width: 35.67%;
  z-index: 2;
}

.comparison-image .comparison-image-sub{
  color:#E0004D;
  font-size: 20px;
  line-height: 1.2;
}

.comparison-image .comparison-image-title{
  color: #190A0A;
  font-size: 48px;
  line-height: 1.2;
  margin-top: 14px;
}

.comparison-image .comparison-image-p{
  color: #3F3636;
  font-size: 16px;
  line-height: 1.5;
  margin-top: 25px;
}

.comparison-image .comparison-image-btn{
  position: absolute;
  z-index: 3;
  /* width: 38px; */
  height: 48px;
  border-radius: 48px;
  gap:10px;
  padding:0 10px 0 20px;
  border: 0.5px solid #CCC;
  background: rgba(0, 0, 0, 0.3);
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  color: #FFF;
  font-size: 16px;
  font-family: var(--text-font-family);
  line-height: 1.5;
}

.comparison-image .comparison-image-btn svg{
  display: block;
}

.comparison-image .comparison-image-btn::after,
.comparison-image .comparison-image-btn:before {
  content: "";
  position: absolute;
  display: none;
  width: 160%;
  height: 160%;
  border: 1px solid #fff;
  border-radius: 50%;
  /* animation: bounce-circle 1.8s infinite; */
  box-sizing: border-box;
  left: -30%;
  top: -30%;
  pointer-events: none;
}

.comparison-image .comparison-image-btn::after{
  animation-delay: 400ms;
}

.comparison-image .comparison-image-btn:hover:after,
.comparison-image .comparison-image-btn:hover:before{
  display: none;
}

@keyframes bounce-circle {
  0% {
    opacity: 0;
    transform: scale(0.8);
  }

  20% {
    opacity: 1;
  }

  80%,
  100% {
    opacity: 0;
    transform: scale(1.3);
  }
}

.comparison-image .comparison-image-btn-left{
  left: 49.16%;
  top: 61.48%;
}
.comparison-image .comparison-image-btn-right{
  left: 79.06%;
  top: 32.8%;
}

.comparison-image-product .comparison-image-cont{
  /* max-width: 29.43%; */
  right: auto;
  left: 8.75%;
  top: 31.43%;
}

.comparison-image-product .comparison-image-title{
  font-size: 44px;
}

@media (max-width:1700px) {
  .comparison-image .comparison-image-title{
