.result-new-popup{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 999;
  background: rgba(0, 0, 0, 0);
  display: flex;
  justify-content: flex-end;
  pointer-events: none;
  transition: all .4s ease;
  overflow: hidden;
}
.result-new-popup.active{
  background: rgba(0, 0, 0, .3);
  pointer-events: all;
}

