<style>
.use-step {
  padding: 50px 0;
  position: relative;
  z-index:1;
}

.use-step .use-step-title {
  color: #000;
  font-size: 30px;
  line-height: 1.5;
  font-family: var(--heading-font-family);
  font-weight: normal;
  margin: 0;
  margin-bottom: 30px;
}

.use-step .use-step-cont {
  position: relative;
  padding: 24px 0;
  border-top: 1px solid #190a0a;
  border-bottom: 1px solid #190a0a;
}

.use-step .use-step-jump {
  position: absolute;
  top: -72px;
  right: 0;
  z-index: 2;
  padding: 0 24px;
  height: 42px;
  display: flex;
  align-items: center;
  gap: 4px;
  border-radius: 4px;
  border: 1px solid #e0004d;
  color: #e0004d;
  font-size: 18px;
  line-height: 1;
  cursor: pointer;
}

.use-step .use-step-jump:hover {
  border: 1px solid #BC0047;
  color: #BC0047;
}

.use-step .use-step-jump svg {
  display: block;
  pointer-events: none;
}

.use-step .use-step-jump:hover path{
  stroke:#BC0047;
}

.use-step .use-step-tab {
  display: flex;
  gap: 24px;
  justify-content: center;
  border-bottom: 2px solid #dbdbdb;
}

.use-step .use-step-tab-item {
  height: 75px;
  padding: 0 38px;
  display: flex;
  align-items: center;
  cursor: pointer;
  color: #212121;
  font-size: 26px;
  line-height: 1.35;
  letter-spacing: 1.359px;
  margin-bottom: -2px;
}

.use-step .use-step-tab-item.active {
  font-family: var(--heading-font-family);
  border-bottom: 3px solid #212121;
}

.use-step .use-step-list {
  margin-top: 48px;
}

.use-step .use-step-item {
  display: none;
  position: relative;
  gap: 60px;
}

.use-step .use-step-item.active {
  display: flex;
}

.use-step .use-step-item-text,
.use-step .use-step-item-img {
  width: calc(50% - 30px);
}

.use-step .use-step-item-img-box {
  padding-top: 67.365%;
  overflow: hidden;
  position: relative;
}

.use-step .use-step-item-img-box img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.use-step .use-step-item-text {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-top: 30px;
}

.use-step .other-step .use-step-item-text {
  padding-top: 0;
}

.use-step .use-step-item-text-tag {
  color: #fff;
  font-size: 14px;
  line-height: 1;
  font-family: var(--heading-font-family);
  border-radius: 2px;
  background: #e0004d;
  height: 22px;
  display: inline-flex;
  align-items: center;
  padding: 0 8px;
  margin-bottom: 6px;
}

.use-step .use-step-item-text-title {
  color: #190a0a;
  font-size: 24px;
  line-height: 1.3;
  font-family: var(--heading-font-family);
  font-weight: normal;
  margin: 0;
}

.use-step .use-step-item-text-p {
  color: #190a0a;
  font-size: 16px;
  line-height: 1.8;
  margin-top: 8px;
}

.use-step .use-step-item-text-sub {
  color: #756b6e;
  font-size: 14px;
  line-height: 1;
  margin: 0;
  margin-top: 25px;
  transform: translateY(16px);
}

.use-step .use-step-mode {
  margin-top: 30px;
}
.use-step .use-step-mode-item {
  border-bottom: 1px solid #86868b;
}
.use-step .use-step-mode-title {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px 0;
  cursor: pointer;
}

.use-step .use-step-mode-title-left {
  display: flex;
  align-items: center;
  gap: 8px;
  color: #190a0a;
  font-size: 18px;
  line-height: 1;
  font-family: var(--heading-font-family);
}

.use-step .use-step-mode-show {
  width: 30px;
  height: 30px;
  position: relative;
  cursor: pointer;
}

.use-step .use-step-mode-show::before,
.use-step .use-step-mode-show::after {
  position: absolute;
  content: '';
  top: 50%;
  left: 50%;
  width: 2px;
  height: 14px;
  background-color: #190a0a;
  transform: translate(-50%, -50%);
}

.use-step .use-step-mode-show::after {
  transform: translate(-50%, -50%) rotate(90deg);
}

.use-step .open.use-step-mode-show::before {
  transform: translate(-50%, -50%) rotate(90deg);
}

.use-step .use-step-mode-p {
  color: #190a0a;
  font-size: 14px;
  line-height: 1.7;
  margin: 0;
  margin-top: 5px;
  margin-bottom: 24px;
  display: none;
}

.use-step .use-step-mode-item.active .use-step-mode-p {
  display: block;
}
.use-step .use-step-mode-item.active .use-step-mode-img {
  display: block;
}

.use-step .use-step-mode-img {
  position: absolute;
  top: 0;
  left: calc(50% + 30px);
  width: calc(50% - 30px);
  height: 100%;
  display: none;
}

.use-step .use-step-mode-img-box {
  position: relative;
  width: 100%;
  padding-top: 67.365%;
}

.use-step .use-step-mode-img img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  z-index: 1;
  display: block;
}

.use-step .use-step-mode-label {
  color: #fff;
  font-size: 20px;
  line-height: 1.2;
  font-family: var(--heading-font-family);
  display: flex;
  align-items: center;
  height: 50px;
  padding: 0 16px;
  border-radius: 103px;
  border: 1px solid #fff;
  background: rgba(255, 255, 255, 0.4);
  backdrop-filter: blur(2.6px);
  position: absolute;
  top: 10px;
  left: 10px;
  z-index: 2;
}

.use-step .use-step-video {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  z-index: 2;
}

.use-step .mb-box{
  display:none;
}
@media (min-width: 991px) {
  .use-step .use-step-item-text-top{
    min-height:350px;
  }
}
  
@media (max-width: 1400px) {
  .use-step .use-step-item {
    gap: 40px;
  }
  .use-step .use-step-item-text,
  .use-step .use-step-item-img {
    width: calc(50% - 20px);
  }
}

@media (max-width: 991px) {
  .use-step {
    padding-bottom: 0;
    overflow: hidden;
  }

  .use-step .use-step-title {
    font-size: 28px;
    line-height: 1.25;
  }

  .use-step .use-step-jump {
    height: 30px;
    padding: 0 18px;
    font-size: 13px;
    gap: 0;
    top: -60px;
  }

  .use-step .use-step-jump svg {
    width: 18px;
  }

  .use-step .use-step-cont {
    padding-top: 8px;
  }

  .use-step .use-step-tab {
    gap: 4px;
    border-width: 1px;
    position: relative;
  }
  .use-step .use-step-tab::before {
    content: '';
    position: absolute;
    bottom: -1px;
    left: 50%;
    transform: translateX(-50%);
    width: 100vw;
    height: 1px;
    background-color: #dbdbdb;
    z-index: 0;
  }

  .use-step .use-step-tab-item {
    padding: 0 15px;
    font-size: 14px;
    height: 37px;
    border-width: 2px;
    margin-bottom: 0;
  }

  .use-step .use-step-list {
    margin-top: 24px;
  }

  .use-step .use-step-item {
    flex-direction: column;
    gap: 0px;
  }

  .use-step .use-step-item-text,
  .use-step .use-step-item-img {
    width: 100%;
  }

  .use-step .use-step-item-img{
    margin-top:30px;
  }

  .use-step .use-step-item-text{
    padding-top:0;
  }

  .use-step .use-step-item-text-tag {
    height: 20px;
    font-size: 12px;
    margin-bottom: 4px;
  }

  .use-step .use-step-item-text-p {
    font-size: 14px;
    line-height: 1.57;
    margin-top: 8px;
  }

  .use-step .use-step-item-text-sub {
    margin-top:8px;
    transform: translateY(0px);
    line-height: 1.57;
  }
  .use-step .mb-box{
    display:block;
  }

  .use-step .pc-box{
    display:none;
  }

  .use-step .use-step-mode-img {
    position: relative;
    left: auto;
    top: auto;
    width: 100%;
    height: auto;
    padding-bottom: 10px;
  }

  .use-step .use-step-mode {
    margin-top: 24px;
  }

  .use-step .use-step-mode-item:last-child {
    border: 0;
  }

  .use-step .use-step-mode-title-left img {
    width: 23px;
  }

  .use-step .use-step-mode-title-left {
    font-size: 20px;
    gap: 12px;
  }

  .use-step .use-step-mode-p {
    margin-top: 0;
    line-height: 1.5;
  }

  .use-step .use-step-mode-label {
    height: 30px;
    padding: 0 12px;
    font-size: 12px;
  }
}
</style>
<section class="section-box">
