                  {%- endif -%}
                  {%- if block.settings.text3 != blank -%}
                    <p class="use-step-item-text-p">{{ block.settings.text3 }}</p>
                  {%- endif -%}
                </div>
                {%- if block.settings.text4 != blank -%}
                  <p class="use-step-item-text-sub pc-box">{{ block.settings.text4 }}</p>
                {%- endif -%}
