<script>
  document.querySelector('#shopify-section-{{ section.id }} .use-step .use-step-jump').addEventListener('click', (event) => {
    const targetId = event.target.getAttribute('data-id');
    const targetElement = document.getElementById(targetId);
    if (targetElement) {
      targetElement.scrollIntoView({
        behavior: 'smooth',
        block: 'start'
      });
    }
  });
  
  document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-tab-item').forEach((item, index) => {
    item.addEventListener('click', () => {
      document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-tab-item').forEach(b => b.classList.remove('active'));
      document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-tab-item')[index].classList.add('active');
      document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-item').forEach(b => b.classList.remove('active'));
      document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-item')[index].classList.add('active');
    });
  });

  document.querySelectorAll('#shopify-section-{{ section.id }} .use-step .use-step-mode-title').forEach(item => {
    item.addEventListener('click', function () {
      let parentItem = this.closest('.use-step-mode-item');
      if (window.innerWidth < 991) {
        this.querySelector('.use-step-mode-show').classList.toggle('open');
      } else {
        this.querySelector('.use-step-mode-show').classList.add('open');
      }
      if (parentItem) {
        if (window.innerWidth < 991) return parentItem.classList.toggle('active');
        parentItem.classList.add('active');
        parentItem.parentElement.querySelectorAll('.use-step-mode-item').forEach(sibling => {
          if (sibling !== parentItem) {
            sibling.classList.remove('active');
            sibling.querySelector('.use-step-mode-show').classList.remove('open');
          }
        });
      }
    });
  });
</script>
