<style>
.officially-features{
  position: relative;
  height: 260vh;
}

.officially-features .officially-features-cont{
  position: sticky;
  height: 100vh;
  top: 0;
  overflow: hidden;
}

.officially-features .text-mark{
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100vh;
  padding-top: 80px;
  top: 0;
  z-index: 0;
}

.officially-features .text-mark-txt{
  font-size: 161px;
  color: transparent;
  text-align: center;
  line-height: 1.1;
  background-clip: text;
  -webkit-background-clip: text;
  background-repeat: no-repeat;
  background-image: url({{ section.settings.image | image_url }});
  background-size:100% 200%;
  background-position: 0% 0%;
}
.officially-features .text-mark-txt p{
  margin: 0;
}
.officially-features .product-img{
  position: absolute;
  top: 100%;
  right: 100%;
  width: 65.42vw;
  height: auto;
  z-index: 1;
}

.officially-features .mb-box{
  display: none;
}

.officially-features .officially-features-title{
  color: #190A0A;
  font-size: 50px;
  line-height: 1.2;
}

.officially-features .officially-features-sub{
  color: #190A0A;
  font-size: 18px;
  line-height: 1.5;
  margin-top: 12px;
}

.officially-features .officially-features-sub p{
  margin: 0;
}

.officially-features .officially-features-sub strong{
  font-weight: normal;
  font-family: var(--heading-font-family);
}

.officially-features .officially-features-two{
  position: absolute;
  left: 62%;
  top: 42%;
  max-width: 455px;
  z-index: 4;
  transform: translateY(30px);
  opacity: 0;
}

.officially-features .officially-features-two .svg-icon-box{
  position: absolute;
  right: 108%;
  top: 50%;
}

.officially-features .officially-features-three{
  position: absolute;
  left: 20%;
  top: 40%;
  max-width: 455px;
  z-index: 4;
  transform: translateY(30px);
  opacity: 0;
  display: none;
}

.officially-features .officially-features-three .svg-icon-box{
  position: absolute;
  left: 100%;
  top: 103%;
}

.officially-features .officially-features-btn{
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  width: 240px;
  height: 48px;
  border-radius: 48px;
  border: 1px solid #3F3636;
  margin-top: 30px;
  color: #3F3636;
  font-size: 16px;
  line-height: 1.5;
}

.officially-features .officially-features-three .officially-features-btn{
  width: 217px;
}

.officially-features-popup{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 999;
  background: rgba(0, 0, 0, 0);
  display: flex;
  justify-content: flex-end;
  /* opacity: 0; */
  pointer-events: none;
  transition: all .4s ease;
  overflow: hidden;
}
.officially-features-popup.active{
  background: rgba(0, 0, 0, .3);
  pointer-events: all;
}

.officially-features-popup .officially-features-popup-mark{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height:100%;
  z-index:1;
}

.officially-features-popup .officially-features-popup-cont{
  width: 100%;
  height: 100%;
  max-width: 1280px;
  background: #FBFAF4;
  padding: 80px 15px 80px 60px;
  position: relative;
  z-index:2;
  transform: translateX(100%);
  transition: all .4s ease;
}

.officially-features-popup.active .officially-features-popup-cont{
  transform: translateX(0);
  transition: all .4s ease .1s;
}

.officially-features-popup .officially-features-popup-det{
  height: 100%;
  padding-right: 185px;
  overflow-y: auto;
  display: flex;
  gap: 145px;
  color: #3F3636;
  position: relative;
}
.officially-features-popup .officially-features-popup-det::-webkit-scrollbar {
    width: 10px;
}

.officially-features-popup .officially-features-popup-det::-webkit-scrollbar-track {
    background-color: none;
}

.officially-features-popup .officially-features-popup-det::-webkit-scrollbar-thumb {
    background-color: rgba(228, 221, 221, .3);
    border-radius: 5px;
}

.officially-features-popup .officially-features-popup-det::-moz-scrollbar {
  width: 10px;
}

.officially-features-popup .officially-features-popup-det::-moz-scrollbar-track {
  background: none;
}

.officially-features-popup .officially-features-popup-det::-moz-scrollbar-thumb {
  background: rgba(228, 221, 221, .3);
  border-radius: 5px;
}

.officially-features-popup .officially-features-popup-close{
  position: absolute;
  top: 80px;
  right: 70px;
  cursor: pointer;
  z-index: 3;
}

.officially-features-popup .officially-features-popup-left{
  width: 115px;
  flex-shrink: 0;
}

.officially-features-popup .officially-features-popup-sub{
  font-size: 24px;
  line-height: 1.2;
}

.officially-features-popup .officially-features-popup-title{
  font-size: 40px;
  line-height: 1.2;
}

.officially-features-popup .officially-features-popup-p{
  font-size: 18px;
  line-height: 1.2;
  margin-top: 20px;
}

.officially-features-popup .officially-features-popup-video{
  position: relative;
  margin-top: 60px;
}

.officially-features-popup .officially-features-popup-video .img{
  display: block;
  width: 100%;
  position: relative;
  z-index: 1;
}

.officially-features-popup .product-video{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 2;
}
.officially-features-popup .product-video video{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.officially-features-popup .officially-features-popup-list{
  display: flex;
  flex-wrap: wrap;
  max-width: 330px;
  gap: 60px;
  row-gap: 40px;
  margin-top: 60px;
}

.officially-features-popup .officially-features-popup-item{
  width: calc(50% - 30px);
}

.officially-features-popup .officially-features-popup-t{
  color: #6C6AB4;
  font-size: 48px;
  line-height: 1.2;
}

.officially-features-popup .officially-features-popup-d{
  color: #3F3636;
  font-size: 14px;
  line-height: 1.5;
}

.officially-features-popup-two .officially-features-popup-cont{
  background: linear-gradient(63deg, #30323E 12.03%, #30323E 27.24%, #14141E 78.01%);
}

.officially-features-popup-two .officially-features-popup-det{
  color: #fff;
}

.officially-features-popup-two .officially-features-popup-left{
  width: 130px;
}

.officially-features-popup-two .officially-features-popup-list{
  max-width: none;
  gap: 140px;
  row-gap: 40px;
  margin-top: 30px;
}

.officially-features-popup-two .officially-features-popup-item{
  width: auto;
}

.officially-features-popup-two .officially-features-popup-t{
  color: #D2FFFC;
}

.officially-features-popup-two .officially-features-popup-d{
  color: #FBFAF4;
}

.officially-features-popup-two .product-img-two{
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  z-index: -1;
}

.officially-features-popup-two .officially-features-popup-det{
  overflow: hidden;
  height: auto;
}

.officially-features-popup-two .officially-features-popup-title{
  margin-bottom: 63%;
}

.officially-features-popup-two .officially-features-popup-cont{
  overflow-y: auto;
}

.officially-features-popup-two .officially-features-popup-cont::-webkit-scrollbar {
    width: 10px;
}

.officially-features-popup-two .officially-features-popup-cont::-webkit-scrollbar-track {
    background-color: none;
}

.officially-features-popup-two .officially-features-popup-cont::-webkit-scrollbar-thumb {
    background-color: rgba(228, 221, 221, .3);
    border-radius: 5px;
}

.officially-features-popup-two .officially-features-popup-cont::-moz-scrollbar {
  width: 10px;
}

.officially-features-popup-two .officially-features-popup-cont::-moz-scrollbar-track {
  background: none;
}

.officially-features-popup-two .officially-features-popup-cont::-moz-scrollbar-thumb {
  background: rgba(228, 221, 221, .3);
  border-radius: 5px;
}

.officially-features-popup-svg{
  padding-left: 23%;
}

@media (max-width:1280px) {
  .officially-features-popup .officially-features-popup-det{
    gap: 60px;
    padding-right: 100px;
  }
  .officially-features-popup .officially-features-popup-title{
    font-size: 35px;
  }
  .officially-features-popup .officially-features-popup-t{
    font-size: 38px;
  }
  .officially-features-popup-two .officially-features-popup-list{
    gap: 60px;
  }
  .officially-features-popup-svg{
    padding-left: 15%;
  }
  .officially-features-popup-two .officially-features-popup-title{
    margin-bottom: 55%;
  }
}

@media (max-width:991px) {
  .officially-features-popup{
    display: flex;
    align-items: flex-end;
  }
  
  .officially-features-popup .officially-features-popup-cont{
    max-height: 100%;
    height: auto;
    transform: translateY(100%);
    overflow-x: hidden;
  }

  .officially-features-popup.active .officially-features-popup-cont{
    transform: translateY(0);
  }
  .officially-features-popup .officially-features-popup-close{
    top: 18px;
    right: 18px;
  }
  .officially-features-popup .officially-features-popup-det{
    flex-direction: column;
    gap: 10px;
  }
  .officially-features-popup .officially-features-popup-left{
    width: auto;
  }
  .officially-features-popup .officially-features-popup-sub{
    font-size: 14px;
  }
  .officially-features-popup .officially-features-popup-title{
    font-size: 24px;
  }
  .officially-features-popup .officially-features-popup-video{
    margin-top: 35px;
  }
  .officially-features-popup.active .officially-features-popup-cont{
    padding: 50px 20px;
  }
  .officially-features-popup .officially-features-popup-det{
    padding-right: 0;
  }
  .officially-features-popup .officially-features-popup-list{
    margin-top: 45px;
    gap: 50px;
  }
  .officially-features-popup .officially-features-popup-t{
    font-size: 35px;
  }
  .officially-features-popup .officially-features-popup-d{
    font-size: 12px;
  }
  .officially-features-popup-two .product-img-two{
    max-width: none;
    width: 220%;
    left: 50%;
    transform: translateX(-50%);
    top: -10%;
  }
  .officially-features-popup-two .officially-features-popup-list{
    row-gap: 20px;
    gap: 0;
    margin-top: 0;
    justify-content: space-between;
  }
  .officially-features-popup-two .officially-features-popup-title{
    margin-bottom: 60%;
  }
}

@media (max-width:1920px) and (min-width:1600px) {
  .officially-features .officially-features-three .svg-icon-box {
    position: absolute;
    left: 80%;
    top: 75%;
  }
  .officially-features .officially-features-two .svg-icon-box {
    position: absolute;
    right: 105%;
    top: 36%;
  }
}
  
@media (max-width:1600px) {
  .officially-features .text-mark{
    align-items: flex-start;
  }
  .officially-features .officially-features-three{
    left: 10%;
    top: 30%;
  }
  .officially-features .officially-features-three .svg-icon-box {
    position: absolute;
    left: 85%;
    top: 80%;
  }
}

@media (max-width:1540px) {
  .officially-features .officially-features-two{
    top: 30%;
  }
}

@media (max-width:1400px) {
  .officially-features .text-mark-txt{
    font-size: 120px;
  }
}
@media (max-width:1100px) {
  .officially-features .officially-features-two,
  .officially-features .officially-features-three{
    max-width: 350px;
  }
  .officially-features .officially-features-title{
    font-size: 32px;
  }
  .officially-features .officially-features-sub{
    font-size: 14px;
  }
  .officially-features .officially-features-btn{
    font-size: 14px;
    height: 40px;
  }
  .officially-features .officially-features-two{
    left: 52%;
    top: 34%;
  }
  .officially-features .officially-features-two .svg-icon-box{
    zoom: .6;
  }
}

@media (max-width:991px) {
  .officially-features .pc-box{
    display: none;
  }

  .officially-features .mb-box{
    display: block;
  }

  .officially-features .text-mark-txt{
    font-size: 85px;
  }

  .officially-features .product-img{
    max-width: none;
    width: 200vw;
  }

  .officially-features .officially-features-two{
    max-width: 238px;
    left: 22%;
    top: 56%;
  }

  .officially-features .officially-features-three{
    max-width: 255px;
    left: 38px;
    top: 60%;
  }

  .officially-features .officially-features-title{
    font-size: 24px;
  }

  .officially-features .officially-features-sub{
    font-size: 12px;
  }

  .officially-features .officially-features-btn{
    font-size: 12px;
    width: 189px;
    margin-top: 20px;
  }

  .officially-features .officially-features-three .officially-features-btn{
    width: 171px;
  }

  .officially-features .officially-features-two .svg-icon-box{
    zoom: 1;
    right: 40%;
    top: auto;
    bottom: 115%;
  }

  .officially-features .officially-features-three .svg-icon-box{
    left: 0;
    top: auto;
    bottom: 105%;
  }

}
</style>

