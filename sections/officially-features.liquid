<!-- <script src="https://cdn.jsdelivr.net/npm/gsap@3.12.5/dist/gsap.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/gsap@3.12.5/dist/ScrollTrigger.min.js"></script>             -->
<script>
  function featuresScrollTo(){
    let pam = {
      right1:'93%',
      right2:'168%',
      top1:'-67%',
      top2:'-113%'
    }
    if(window.innerWidth<1921){ //1920 * 1080
      pam.right1 = '89%'
      pam.right2 = '178%'
      pam.top1 = '-64%'
      pam.top2 = '-112%'
    }
    
    if(window.innerWidth<1601){ //1600 * 900
      pam.right1 = '83%'
      pam.right2 = '167%'
      pam.top1 = '-65%'
      pam.top2 = '-120%'
    }
    if(window.innerWidth<1537){ //1536 * 695
      pam.right1 = '82%'
      pam.right2 = '170%'
      pam.top1 = '-63%'
      pam.top2 = '-108%'
    }
    if(window.innerWidth<1500){ //1440 * 900
      pam.right1 = '80%'
      pam.right2 = '175%'
      pam.top1 = '-80%'
      pam.top2 = '-123%'
    }
    if(window.innerWidth<1370){ //1366 * 768
      pam.right1 = '77%'
      pam.right2 = '180%'
      pam.top1 = '-72%'
      pam.top2 = '-115%'
    }
    if(window.innerWidth<1281){ // 1280 * 551
      pam.right1 = '76%'
      pam.right2 = '185%'
      pam.top1 = '-78%'
      pam.top2 = '-118%'
    }
    if(window.innerWidth<1025){ //1024 * 600
      pam.right1 = '68%'
      pam.right2 = '186%'
      pam.top1 = '-71%'
      pam.top2 = '-116%'
    }
    if(window.innerWidth<992){
      pam.right1 = '36%'
      pam.right2 = '108%'
      pam.top1='-90%',
      pam.top2='-145%'
    }
    ScrollTrigger.create({
      trigger: ".officially-features",
      start: "top top",
      end: "+=160%",
      scrub: true,
      // pin: true,
      animation:
      gsap.timeline()
      .to(".text-mark-txt",{'background-position': '0% 100%','opacity':0,ease:'power2.inOut'})
      .to(".product-img",{'transform': 'translate('+pam.right1+','+pam.top1+')',ease:'power2.inOut'},'<')
      .to(".officially-features-two",{'transform': 'translateY(0)','opacity':1,ease:'power2.inOut'})
      .to(".officially-features-two",{'transform': 'translateY(-30px)','opacity':0,ease:'power2.inOut'})
      .to(".product-img", {'transform': 'translate('+pam.right2+','+pam.top2+')'})
      .to(".officially-features-three",{'display':'block','transform': 'translateY(0)','opacity':1,ease:'power2.inOut'})
    })
   }
   
  window.addEventListener('load', function(){
    featuresScrollTo();
    $('.officially-features-btn').click(function(){
      let name = $(this).data('popup')
      let $p =  $('.'+name)
      $p.addClass('active')
      if($p.find('.product-video .video-wrapper__play-button').length > 0){
        // $p.find('.officially-features-popup-video video').get(0).play()
        $p.find('.product-video .video-wrapper__play-button').click()
        // $('.officially-features-popup-wrapper video').get(0).play()
      }
      $('body').css('overflow','hidden')
    })
    $('.officially-features-popup-close,.officially-features-popup-mark').click(function(){
      let $p =  $('.officially-features-popup')
      $p.removeClass('active')
      // if($p.find('.officially-features-popup-video video').length > 0){
      //   $p.find('.officially-features-popup-video video').get(0).pause()
      //   $p.find('.officially-features-popup-video video').get(0).currentTime = 0
      // }
      $('body').css('overflow','')
    })
  })
</script>
