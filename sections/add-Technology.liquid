<link rel="stylesheet" href="//www.ulike.com/cdn/shop/t/116/assets/swiper-bundle.min.css">
<script src="//www.ulike.com/cdn/shop/t/116/assets/swiper-bundle.min.aio.min.js"></script>
<style>
.add-Technology {
  padding: 80px 0;
  background: linear-gradient(180deg, rgba(236, 239, 242, 1) 8%, #fff 92%);
  overflow: hidden;
}

.add-Technology .add-Technology-top {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}

.add-Technology .add-Technology-top-title {
  color: #000;
  font-size: 60px;
  line-height: 1.2;
  font-family: var(--heading-font-family);
  font-weight: normal;
  margin: 0;
}

.add-Technology .add-Technology-top-p {
  color: #000;
  font-size: 18px;
  line-height: 1.2;
  max-width: 540px;
  margin: 0;
  margin-top: 20px;
}

.add-Technology .add-Technology-box {
  position: relative;
  padding-top: 1px;
}

.add-Technology .add-Technology-top-arraws {
  display: flex;
  align-items: center;
  gap: 40px;
  position: absolute;
  right: 0;
  top: -30px;
  z-index: 3;
}

.add-Technology .add-Technology-top-arraw {
  width: 40px;
  cursor: pointer;
  outline: 0;
}

.add-Technology .swiper-button-disabled {
  opacity: 0.25;
}

.add-Technology .add-Technology-top-arraw svg {
  display: block;
  width: 100%;
  height: auto;
}

.add-Technology .dd-Technology-cont {
  margin-top: 50px;
}

.add-Technology .dd-Technology-list {
  display: flex;
}

.add-Technology .dd-Technology-item {
  flex-shrink: 0;
  width: 49.34%;
  position: relative;
  border-radius: 10px;
  overflow: hidden;
}

.add-Technology .dd-Technology-item .img {
  display: block;
  width: 100%;
  position: relative;
  z-index: 1;
}

.add-Technology .img-icon {
  display: block;
  width: 70px;
}

.add-Technology .dd-Technology-item-text {
  position: absolute;
  left: 50%;
  bottom: 40px;
  transform: translateX(-50%);
  width: calc(100% - 80px);
  z-index: 2;
  padding-bottom: 20px;
  border-bottom: 2px solid #fff;
}

.add-Technology .dd-Technology-item-title {
  color: #fff;
  font-size: 30px;
  line-height: 1.2;
  font-family: var(--heading-font-family);
  font-weight: normal;
  margin: 0;
  margin-top: 10px;
}

.add-Technology .dd-Technology-item-p {
  color: #fff;
  font-size: 16px;
  line-height: 1.2;
  margin: 0;
  margin-top: 20px;
}

@media (max-width: 1400px) {
  .add-Technology .add-Technology-top-title {
    font-size: 40px;
  }
  .add-Technology .add-Technology-top-p {
    font-size: 14px;
    margin-top: 10px;
  }
  .add-Technology .dd-Technology-item-title {
    font-size: 24px;
  }
  .add-Technology .dd-Technology-item-p {
    font-size: 14px;
    margin-top: 10px;
  }
  .add-Technology .dd-Technology-item-text {
    width: calc(100% - 40px);
    bottom: 20px;
  }
  .add-Technology .img-icon {
    width: 50px;
  }
}

@media (max-width: 991px) {
  .add-Technology {
    padding: 50px 0;
  }

  .add-Technology .add-Technology-top {
    flex-direction: column;
    align-items: center;
    text-align: center;
  }

  .add-Technology .add-Technology-top-title {
    font-size: 28px;
  }
  .add-Technology .dd-Technology-cont {
    margin-top: 30px;
  }
  .add-Technology .dd-Technology-item {
    width: 100%;
    max-width: 450px;
  }
  .add-Technology .add-Technology-box {
    padding-bottom: 50px;
  }
  .add-Technology .add-Technology-top-arraws {
    top: auto;
    bottom: 0;
    right: 50%;
    transform: translateX(50%);
    gap: 20px;
  }
  .add-Technology .add-Technology-top-arraw {
    width: 30px;
  }
  .add-Technology .img-icon {
    width: 40px;
  }
  .add-Technology .dd-Technology-item-title {
    font-size: 18px;
  }
  .add-Technology .dd-Technology-item-p {
    font-size: 12px;
    margin-top: 5px;
  }
  .add-Technology .dd-Technology-item-text {
    padding-bottom: 10px;
  }
}
</style>
<section class="section-box">
  <div class="add-Technology">
    <div class="container">
      <div class="add-Technology-top">
        <div class="add-Technology-top-text">
          {%- if section.settings.text != blank -%}
            <h3 class="add-Technology-top-title">{{ section.settings.text }}</h3>
          {%- endif -%}
          {%- if section.settings.text1 != blank -%}
            <p class="add-Technology-top-p">{{ section.settings.text1 }}</p>
          {%- endif -%}
        </div>
      </div>
      <div class="add-Technology-box">
        <div class="dd-Technology-cont">
          <div class="dd-Technology-list swiper-wrapper">
            {%- for block in section.blocks -%}
              <div class="dd-Technology-item swiper-slide">
                {%- if block.settings.image != blank -%}
                    <img
                      loading="lazy"
                      class="img"
                      width="{{ block.settings.image.width }}"
                      height="{{ block.settings.image.height }}"
                      {% render 'image-attributes', image: block.settings.image %}
                      alt="{{ block.settings.image.alt | escape }}"
                    >
                {%- endif -%}
                <div class="dd-Technology-item-text">
                  {%- if block.settings.image1 != blank -%}
                    <img
                      loading="lazy"
                      class="img-icon"
                      width="{{ block.settings.image1.width }}"
                      height="{{ block.settings.image1.height }}"
                      {% render 'image-attributes', image: block.settings.image1 %}
                      alt="{{ block.settings.image1.alt | escape }}"
                    >
                  {%- endif -%}
                  {%- if block.settings.text != blank -%}
                    <h3 class="dd-Technology-item-title">{{ block.settings.text }}</h3>
                  {%- endif -%}
                  {%- if block.settings.text1 != blank -%}
                    <p class="dd-Technology-item-p">{{ block.settings.text1 }}</p>
                  {%- endif -%}
                </div>
              </div>
            {%- endfor -%} 
          </div>
        </div>
        <div class="add-Technology-top-arraws">
          <div class="add-Technology-top-arraw add-Technology-top-arraw-prev">
            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="23" viewBox="0 0 30 23" fill="none">
              <g clip-path="url(#clip0_940_2652)">
                <path
                  d="M1.90735e-06 11.4915C1.90735e-06 11.6729 0.067318 11.8529 0.196098 11.9905L10.44 22.9661L11.5098 21.9681L2.41463 12.2232H30C30 11.4154 29.3444 10.7598 28.5366 10.7598H0.765366C0.488781 10.7598 0.219515 10.8988 0.0878067 11.1432C0.0278072 11.2529 1.90735e-06 11.3729 1.90735e-06 11.4915Z"
                  fill="#190A0A" />
                <path
                  d="M1.53073 8.92683C2.10292 9.49756 3.02926 9.49756 3.59999 8.92683L11.4937 1.03463L10.459 0L1.53219 8.92683H1.53073Z"
                  fill="#190A0A" />
              </g>
              <defs>
                <clipPath id="clip0_940_2652">
                  <rect width="30" height="22.9683" fill="white" transform="matrix(-1 0 0 1 30 0)" />
                </clipPath>
              </defs>
            </svg>
          </div>
          <div class="add-Technology-top-arraw add-Technology-top-arraw-next">
            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="23" viewBox="0 0 30 23" fill="none">
              <g clip-path="url(#clip0_940_2648)">
                <path
                  d="M30 11.4915C30 11.6729 29.9327 11.8529 29.8039 11.9905L19.56 22.9661L18.4902 21.9681L27.5854 12.2232H0C0 11.4154 0.65561 10.7598 1.46341 10.7598H29.2346C29.5112 10.7598 29.7805 10.8988 29.9122 11.1432C29.9722 11.2529 30 11.3729 30 11.4915Z"
                  fill="#190A0A" />
                <path
                  d="M28.4693 8.92683C27.8971 9.49756 26.9707 9.49756 26.4 8.92683L18.5063 1.03463L19.541 0L28.4678 8.92683H28.4693Z"
                  fill="#190A0A" />
              </g>
              <defs>
                <clipPath id="clip0_940_2648">
                  <rect width="30" height="22.9683" fill="white" />
                </clipPath>
              </defs>
            </svg>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<script>
  let addTechnology = new Swiper('.dd-Technology-cont', {
    slidesPerView: "auto",
    spaceBetween: 40,
    navigation: {
      nextEl: '.add-Technology-top-arraw-next',
      prevEl: '.add-Technology-top-arraw-prev',
    }
  });
</script>
{% schema %}
{
  "name": "add-Technology",
  "blocks": [
    {
      "name": "item",
      "type": "item",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "image_picker",
          "id": "image1",
          "label": "Icon"
        },
        {
          "type": "text",
          "id": "text",
          "label": "Text"
        },
        {
          "type": "text",
          "id": "text1",
          "label": "Text"
        }
      ]
    }
  ],
  "settings": [
    {
      "type": "text",
      "id": "text",
      "label": "Text"
    },
    {
      "type": "text",
      "id": "text1",
      "label": "Text"
    }
  ],
  "presets": [
    {
      "name": "add-Technology",
      "blocks": []
    }
  ]
}
{% endschema %}
