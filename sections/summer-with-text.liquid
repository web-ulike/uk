<script>
  window.addEventListener('load', () => {
    $('#shopify-section-{{ section.id }} .btn-popup-test').click(function () {
      $('#shopify-section-{{ section.id }} .officially-test-popup').addClass('active');
      $('body').css('overflow', 'hidden');
    });
    $(
      '#shopify-section-{{ section.id }} .officially-test-popup-close,#shopify-section-{{ section.id }} .officially-test-popup-mark'
    ).click(function () {
      $('#shopify-section-{{ section.id }} .officially-test-popup').removeClass('active');
      $('body').css('overflow', '');
    });
    function initSelect() {
      let $p = $('#shopify-section-{{ section.id }} .officially-test-popup');
      let skinArr = [0, 1, 2, 3];
      let hairArr = [0, 1, 2, 3];
      let skinIndex = null;
      let hairIndex = null;

      // 获取选中项对应的结果
      function getResults() {
        return skinArr.includes(skinIndex) && hairArr.includes(hairIndex);
      }

      // 重新测试
      function resetAll() {
        skinIndex = null;
        hairIndex = null;
        $p.find('.officially-test-popup-btn').css('display', 'none');
        $p.find('.officially-test-popup-submit').removeClass('active').css('display', 'flex');
        $p.find('.officially-test-popup-item').removeClass('active');
        $p.find('.officially-test-popup-results-item').css('display', 'none');
        $p.find('.officially-test-popup-lebel-span').attr('style', '');
        $p.find('.officially-test-popup-msg').css('display', 'block');
      }
      // 点击选项
      $p.find('.officially-test-popup-item').click(function () {
        if ($p.find('.officially-test-popup-submit').css('display') == 'none') return;
        let $this = $(this);
        let $par = $this.parents('.officially-test-popup-list');
        let type = $par.data('type');
        let index = $this.index();
        let src = $this.find('.popup-img').attr('src');
        $this.addClass('active').siblings().removeClass('active');
        if (type == 'skin') {
          skinIndex = index;
        } else {
          hairIndex = index;
        }
        $par
          .siblings('.officially-test-popup-lebel')
          .find('.officially-test-popup-lebel-span')
          .css({ background: 'url(' + src + ') no-repeat', 'background-size': 'cover' });
        if (skinIndex != null && hairIndex != null) {
          $p.find('.officially-test-popup-submit').addClass('active');
        }
      });
      // 提交反馈结果
      $p.find('.officially-test-popup-submit').click(function () {
        let $this = $(this);
        if ($this.hasClass('active')) {
          $p.find('.officially-test-popup-results-item').css('display', 'none');
          $this.css('display', 'none');

          if (getResults()) {
            $p.find('.officially-test-popup-results-item').eq(0).css('display', 'flex');
            $this
              .siblings('.officially-test-popup-shop,.officially-test-popup-reset:not(.active)')
              .css('display', 'flex');
          } else {
            $p.find('.officially-test-popup-results-item').eq(1).css('display', 'flex');
            $this.siblings('.officially-test-popup-reset.active').css('display', 'flex');
          }
          $this.parents('.officially-test-popup-btn-box').siblings('.officially-test-popup-msg').css('display', 'none');
        }
      });
      // 重新测试
      $p.find('.officially-test-popup-reset').click(function () {
        resetAll();
      });

      // Back to Shop
      $p.find('.officially-test-popup-shop').click(function () {
        $p.removeClass('active');
        $('body').css('overflow', '');
        resetAll();
      });
    }
    initSelect();
  });
</script>
