  <div class="navs nav1 navp-1 pc-show">
    <div class="box">
      <div class="nav">
        <h2 class="to-top">{{ section.settings['modle1-title'] }}</h2>
        <p class="to-top">
          {{ section.settings['modle1-desc'] }}
        </p>

        {{-
          section.settings['modle1-icon']
          | image_url: width: section.settings['modle1-icon'].width
          | image_tag:
            loading: 'lazy',
            draggable: false,
            alt: section.settings['modle1-icon'].name,
            class: 'rtcon to-top'
        -}}
        <video
          id="myVideo"
          class="to-top"
          src="{{section.settings.modle1-video }}"
          preload="metadata"
          autoplay
          muted
          loop
          webkit-playsinline="true"
          x5-playsinline="x5-playsinline"
          playsinline="true"

          x-webkit-airplay="true"
        ></video>
      </div>
      <div class="ftnav to-top">
        <div class="left">
          <h3>{{ section.settings['modle1-label'] }}</h3>
          <p>
            {{ section.settings['modle1-describe'] }}
          </p>
        </div>
        <ul class="ul1 right">
          {% for item in se_bks %}
            {% if item.type == 'model-right' %}
          <li>
            <span> {{ item.settings.describe }} </span>
            <i>{{ item.settings.name }}</i>
          </li>
        {% endif %}
        {% endfor %}
        
        </ul>
      </div>
    </div>
  </div>
