      bottom: 30px;
      color: var(--Ulike, #3f3636);

      font-size: 18px;

      font-weight: 300;
      line-height: 27px;
    }
    .nav3 {
      background: linear-gradient(180deg, #FFF 0%, #EAE7FF 50%, #FFF 100%);
      {% comment %} background-attachment: fixed; {% endcomment %}
      padding-bottom: 0;
    }

    .nav3img {
      height: 1700px;
      background: url('https://cdn.shopify.com/s/files/1/0740/5882/6015/files/Rectangle_305_2.png?v=1720608175') #fff  no-repeat right top; position: relative;
      position: relative;

      background-size: 80% auto;

      background-attachment: fixed;
    }

    .nav3_1 {
      position: absolute;
      left: 7%;
      width: 35%;
      top: 30%;
    }

    .nav3_1 h3 {
      color: #190a0a;

      font-size: 48px;
      margin-bottom:20px;

      line-height: 62.4px;
    }
    .nav3_1 p {
      color: var(--Ulike, #190A0A);
  font-size: 18px;
  line-height:27px;
  text-align:left;
    }
    .nav3_2 {
      width: 30%;
      right: 0;
      position: absolute;
      right: 7%;
      top: 70%;
      color: var(--Ulike, #190a0a);

      font-size: 18px;

      font-weight: 300;
      line-height: 27px;
    }

    .nav4 { 
      background: linear-gradient(180deg, #FFF 5%, #D6FDFF 50%, #FFF 68%);
      {% comment %} background-attachment: fixed; {% endcomment %}
    }
    .nav4 .nav p{
      width:auto;
    }
    .nav5 {
      background: linear-gradient(180deg, #FFF 5%, #FFFBE4 50%, #FFF 68%);
      {% comment %} background-attachment: fixed; {% endcomment %}
    }
    .nav5foot h3{
      color: var(--Ulike, #190A0A);

      font-size: 44px;


      line-height: 57.2px;
      margin-top:59.49px !important;
    }
    .nav5foot p{
      color: var(--Ulike, #190A0A);

    font-size: 18px;

    font-weight: 300;
    line-height: 27px;
    margin:10px  0;
    text-align:left;
    }
    .nav5foot a{
