    @charset "utf-8";
    * {
      box-sizing: border-box;
      margin: 0;
      padding: 0;
    }
    .white{
      background:#fff !important;
    }
    li {
      list-style-type: none;
    }

    body {
      margin: 0;
      padding: 0;
      overflow-x: hidden;
      scroll-behavior: smooth; /* 设置平滑滚动 */
    }

    p {
      margin: 0 0 20px 0;
    }

    img {
      border: 0;
      display: block;
      max-width: 100%;
    }
    .pc-show {
      display: block;
    }
    .m-show {
      display: none;
    }
    .nav {
      overflow: hidden;
    }
    .nav h2 {
      color: var(--Ulike, #190a0a);
      text-align: center;

      font-size: 66px;


      line-height: 79.2px;
      width: 800px;
      margin: 0 auto 40px auto;
    }

    .nav p {
      color: var(--Ulike, #190a0a);
      text-align: center;

      font-size: 30px;


      line-height: 39px;
      width: 800px;
      margin: 0 auto 40px auto;
    }

    .nav .rtcon {
      width: 184px;
      margin: 60px auto 127px auto;
    }

    .nav video {
      width: 100%;
    }

    .nav4foot {
      padding: 50px 0;
      display: flex;

    }

    .nav4foot h3 {
      color: var(--Ulike---Off-Black, #190A0A);
      leading-trim: both;
      text-edge: cap;

      font-size: 44px;


      line-height: 57.2px;

    }

    .nav4foot p {
      margin-top:30px;
    color: var(--Ulike, #190A0A);

