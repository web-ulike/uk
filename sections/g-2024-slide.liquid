      text-align: center;
      leading-trim: both;
      text-edge: cap;

      font-size: 200px;

      line-height: 240px;
    }
   .numberScrolling .numberScrolling-item .numberScrolling-label {
      color: var(--Ulike, #190a0a);
      text-align: center;

      font-size: 24px;


      line-height: 28.8px;
