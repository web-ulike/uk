    .ul2 {
      display: flex;
      justify-content: flex-end;
      margin-left: auto;
      width: 40%;
    }

    .ul2 span {
      font-size: 72px;
      line-height: 1;
      color: #e0004d;
      display: block;
    }

    .ul2 i {
      font-size: 16px;
      font-style: normal;
    }

    .ul2 li {
      padding-left: 50px;
      text-align: center;
    }
    .ul3{
      display: flex;
      justify-content: flex-end;
      margin-left: auto;
      width: 23%;
      flex-wrap:wrap;

      text-align:center;
