<script>
  document.addEventListener('DOMContentLoaded', function () {
    function onBridgeReady() {
      //这个方法在ios能自动播放，在安卓机上还是不可以
      WeixinJSBridge.invoke('getNetworkType', {}, function () {
        $('video')[0].currentTime = 0; //这一句是解决安卓没有自动播放，但是可以加载视频封面出来，不至于显示空白
        $('video')[0].play();
      });
    }
    if (typeof WeixinJSBridge == 'undefined') {
      if (document.addEventListener) {
        setTimeout(function () {
          document.addEventListener('WeixinJSBridgeReady', onBridgeReady(), true);
        }, 1000);
      }
    } else {
      onBridgeReady();
    }

    gsap.registerPlugin(ScrollTrigger);
    gsap.set(".m-main-container", { autoAlpha: 0 });

    ScrollTrigger.create({
      trigger: ".nav2Main",
      markers: false,
      start: "top+=750 center",
      onEnter: () => {
        gsap.to(".m-main-container", { autoAlpha: 1, duration: 1 });
        gsap.to(".nav2img-m ol", { autoAlpha: 0, duration: 1 }); // 隐藏 .nav2img-m ol
      },
      onLeaveBack: () => {
        gsap.to(".m-main-container", { autoAlpha: 0, duration: 1 });
        gsap.to(".nav2img-m ol", { autoAlpha: 1, duration: 1 }); // 显示 .nav2img-m ol
      },
      onUpdate: (self) => {
        const numberScrollingItems = document.querySelectorAll('.numberScrolling-num span');
        const numberScrolling = document.querySelector('.numberScrolling');
    
        const progress = self.progress;
        if (window.innerWidth <= 768) {
          gsap.set(numberScrolling, {
            display: "flex",
          });
          gsap.to(numberScrolling, {
            opacity: 1,
            duration: 2,
            ease: "power2.inOut",
          });
          numberScrollingItems.forEach((item) => {
            const count = parseInt(item.getAttribute("data-count"), 10);
            gsap.to(item, {
              innerText: count,
              snap: "innerText",
              duration: 2,
              ease: "power2.inOut",
            });
          });
        }
      }
    });
    
    // 点击锚点到某个区域
    const fixedButtonContainer = document.getElementById('fixedButtonContainer');
    const anchorGroup = document.getElementById('anchor-group');
    document.querySelectorAll('.anchor').forEach((anchor) => {
      anchor.addEventListener('click', function (e) {
        const targetSelector = this.getAttribute('data-target');
        const targetElement = document.querySelector(targetSelector);
        if (targetElement) {
          window.scrollTo({
            top: targetElement.offsetTop-50,
            behavior: 'smooth',
          });

          // Show the fixed button container
          fixedButtonContainer.style.display = 'block';
        }
      });
    });

    const main2Img = document.getElementById('main2Img');
    const numberScrollingItems = document.querySelectorAll('.numberScrolling-num span');
    const numberScrolling = document.querySelector('.numberScrolling');
    const nav2ImgOl = document.querySelector('.nav2img ol');
    // Initial state: scale the image to 1x
    gsap.set(main2Img, { scale: 1 });
    gsap.set(numberScrolling, { opacity: 0 }); // Set initial opacity to 0
    // ScrollTrigger for scaling and pinning
    let isNumberScrollingTriggered = false;
    let ticking = false;
    ScrollTrigger.create({
      trigger: ".navs.nav2",
      start: "top+=650 top",
      endTrigger: "body",
      end: "+=500",
      scrub: true,
      pin: ".nav2img",
      markers: false,
      toggleActions: "play none none none",
    
      onUpdate: (self) => {
        if (!ticking) {
          ticking = true;
          const direction = self.direction; // 获取滚动方向（1为向下，-1为向上）
          const progress = self.progress;
          let scaleValue = 0
          console.log('window.innerWidth',window.innerWidth)
          if(window.innerWidth <1350 && window.innerWidth > 760){
            scaleValue = direction === 1 ? 4.5 : 1;
          }else {
            scaleValue = direction === 1 ? 4 : 1;
          }
          console.log('scaleValue',scaleValue)
          gsap.to(main2Img, {
            scale: scaleValue,
            duration: 0.3,
            zIndex: 1,
            overwrite: true,
          });

          // 如果是向上滚动，就隐藏 numberScrolling
          if (direction === -1) {
            gsap.set(numberScrolling, {
              display: "none",
            });
            isNumberScrollingTriggered = false; // 重置触发状态
          } else if (progress >= 0.5 && !isNumberScrollingTriggered) {
            if(window.innerWidth >768){
              isNumberScrollingTriggered = true;
              gsap.set(numberScrolling, {
                display: "flex",
              });
              gsap.to(numberScrolling, {
                opacity: 1,
                duration: 2,
                ease: "power2.inOut",
              });
              numberScrollingItems.forEach((item) => {
                const count = parseInt(item.getAttribute("data-count"), 10);
                gsap.to(item, {
                  innerText: count,
                  snap: "innerText",
                  duration: 2,
                  ease: "power2.inOut",
                });
              });
            }
          }
          console.log('scaleValue',scaleValue)
          if (scaleValue >= 3.5) {
            gsap.to(nav2ImgOl, {
              display: "none",
              duration: 0.3,
            });
         
          } else {
            gsap.to(nav2ImgOl, {
              display: "block",
              duration: 0.3,
            });
          
          }
    
          if (progress >= 1) {
             if(window.innerWidth <1400 && window.innerWidth > 768){
              gsap.set(".nav2img img", { clipPath: "inset(0 0 50px 0)" });
            }else if(window.innerWidth <1920 && window.innerWidth > 768){
              gsap.set(".nav2img img", { clipPath: "inset(0 0 100px 0)" });
            }else{
              gsap.set(".nav2img img", { clipPath: "inset(0 0 150px 0)" });
            }
          
          } else {
            gsap.set(".nav2img img", { clipPath: "none" });
          }
    
          ticking = false;
        }
      },
    });
    

    $(window).scroll(function () {
      var h = $('.nav3img').offset().top;
      s = $(window).scrollTop();
      if (s > h) {
        $('.nav3img').css({ backgroundAttachment: 'fixed' });
      } else {
        $('.nav3img').css({ backgroundAttachment: 'unset' });
      }
      var s2 = s + 400;
      if (s2 > h) {
        let mtop = s2 - h;
        $('.nav3_1,.nav3_2').css({ opacity: mtop / 100 });
      }

      document.getElementById('fixedButton').addEventListener('click', function () {
        window.scrollTo({
          top: anchorGroup.offsetTop,
          behavior: 'smooth',
        });
        fixedButtonContainer.style.display = 'none';
      });
    });
     // 监听滚动事件，当距离顶部100像素时逐渐隐藏按钮
     window.addEventListener('scroll', function () {

      if (window.scrollY <= 600) {
        fixedButtonContainer.style.display = 'none';

      } 
    });  
  });
     
</script>
