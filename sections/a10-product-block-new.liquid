<script>
  var pid_{{ section.id | replace: "template--", ""  }} = '{{ product.selected_or_first_available_variant.id }}';
  function isMobileW(){
    return window.innerWidth < 992
  }
  // 图片展示优化 @2024-10-28
  var variantArr_{{ se_id }} = [];
  var productMediaArr_{{ se_id }} = [];
  {% for media in product.media %}
    productMediaArr_{{ se_id }}.push({{ media | json }});
  {% endfor %}

  
  
  {% for variant in product.variants %}
      variantArr_{{se_id }}.push({
        vid: {{ variant.id }},
        position: {{ variant.featured_media.position }},
        url: '{{ variant.metafields.custom["3d_url"] }}',
        page_price: '{{ variant.metafields.custom["page_price"] }}',
        isOriginalPrice: '{{ variant.metafields.custom["isOriginalPrice"] }}',
        original_price: '{{ variant.compare_at_price | money }}',
        price: '{{ variant.price | money }}',
        variantTitle: '{{ variant.metafields.custom["variant_title"] }}',
        title: '{{ product.title }}'
      });
  {% endfor %}
  
  function addMaxProperty(arr) {
    for (let i = 0; i < arr.length; i++) {
        if (i < arr.length - 1) {
          arr[i].max = arr[i + 1].position - 1;
        } else {
          arr[i].max = Number('{{ product.media  | size }}'); // 最后一个对象的 max 被设置为 最大值
        }
    }
    return arr;
  }
  
  variantArr_{{se_id }} = addMaxProperty(variantArr_{{se_id }});



  let galleryThumbs_{{ section.id | replace: "template--", ""  }} = null;
  let galleryTop_{{ section.id | replace: "template--", ""  }} = null;
  function initProductImages_{{se_id }}() {
    if (galleryThumbs_{{ section.id | replace: "template--", ""  }} !== null) {
      galleryThumbs_{{ section.id | replace: "template--", ""  }}.destroy(true, true);
    }
    galleryThumbs_{{ section.id | replace: "template--", ""  }} = new Swiper('#myThumbs_{{ section.id }}', {
      spaceBetween: isMobileW() ? 8 : 15,
      slidesPerView: 'auto',
      lazy: {
        loadPrevNext: true, // 预加载前后两张图片
      },
      freeMode: true,
      watchSlidesVisibility: true,
      watchSlidesProgress: true,
      breakpoints: {
        640: {
          direction: 'horizontal', // 小屏幕上水平方向滚动
        },
        1024: {
          direction: 'vertical', // 大屏幕上垂直方向滚动
        },
      }
    });
    if (galleryTop_{{ section.id | replace: "template--", ""  }} !== null) {
      galleryTop_{{ section.id | replace: "template--", ""  }}.destroy(true, true);
    }
    galleryTop_{{ section.id | replace: "template--", ""  }} = new Swiper('#myGallery_{{ section.id }}', {
      lazy: {
        loadPrevNext: true, // 预加载前后两张图片
      },
      spaceBetween: 10,
      loop: true,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      thumbs: {
        swiper: galleryThumbs_{{ section.id | replace: "template--", ""  }},
      }
    });
  }




  document.addEventListener('DOMContentLoaded', function () {
    generateHtmlForVariant_{{se_id }}();

    //查看更多
    $('#shopify-section-{{ section.id }}').on('click', '.read-more', function () {
      $('.description').toggleClass('expanded');
      // commonGtmEvent('A10_P_read', 'click', '');
      if ($('.description').is('.expanded')) {
        $(this).text('{{se_stts.read-more }} -');
      } else {
        $(this).text('{{se_stts.read-more }} +');
      }
    });

    $('.loox-rating').click(function(){
      $('.jdgm-widget')[0].scrollIntoView({
                behavior: 'smooth',
                block: 'start'
            });
    })

    // 拼团商品
    $('#shopify-section-{{ section.id }}').on('click', '.btn-group-purchase', function () {
      if ($(this).attr('disabled') != 'disabled') {
      let  quantity= $(this).closest('.product-right-panel').find('.inp').val()
      let  giftParam = {};
      {% comment %} 是否有赠品 {% endcomment %}
      {%- if section.settings.is_cart_aloebera -%}
        giftParam = { _aloevera: true };
      {% endif %}
        addToCart(pid_{{ section.id | replace: "template--", ""  }}, $(this)[0], giftParam,'{{ section.settings.text1 }}',quantity);
      }
    });

    //复制折扣码
    $('#shopify-section-{{ section.id }}').on('click', '.copy-right', function () {
     copyCode('{{section.settings.text1}}', "{{section.settings.text2}} : {{section.settings.text1}}", this);
    });

    $('#shopify-section-{{ section.id }}').on('click', '.btn-purchase', function () {
      let  quantity= $(this).closest('.product-right-panel').find('.inp').val()
      let  giftParam = {};
        {% comment %} 是否有赠品 {% endcomment %}
        {%- if section.settings.is_cart_aloebera -%}
          giftParam = { _aloevera: true };
        {% endif %}
        buyProduct(pid_{{ section.id | replace: "template--", ""  }},giftParam,'{{ section.settings.text1 }}',quantity)
    })

    // 加减数量
      $('#shopify-section-{{ section.id }}').on('click','.product-box-num-tap',function () {
        let inp = $(this).siblings('.inp')
        let val = parseInt(inp.val())
        let n = parseInt($(this).data('n'))
        val += n
        if (val < 1) val = 1
        inp.val(val)
      })

      // 控制输入框只能输入大于0的整数
      $('.inp').blur(function () {
        let val = parseInt($(this).val())
        if (isNaN(val) || val < 1) {
          $(this).val(1)
        }
      })

    // 底部置顶栏 添加购物车
    $('#shopify-section-{{ section.id }}').on('click', '.btn-add-cart', function () {
      if ($(this).attr('disabled') != 'disabled') {
        let  giftParam = {};
        {% comment %} 是否有赠品 {% endcomment %}
        {%- if section.settings.is_cart_aloebera -%}
          giftParam = { _aloevera: true };
        {% endif %}
        addToCart(pid_{{ section.id | replace: "template--", ""  }}, $(this)[0], giftParam,{{ section.settings.text1 }});
      }
    });

    // 颜色切换
    $('#shopify-section-{{ section.id }}').on('click', '.product-components_{{ se_id }} .color-swatch', function () {
      var inventory_quantity = $(this).attr('inventory_quantity');
      let _position =  $(this).attr('position') - 1;
      var thatVariant = $(this).closest('.variants-list');
      var colorBlock =  $(this).closest('.color-block');
      let pIindex = $(this).index()
      let diy = $(this).parents('.color-swatch-list').data('diy')
      let num = $(this).parents('.color-swatch-list').data('num')
      // 变体 id
      var vid = $(this).attr('id');
    
      $(this).addClass('active').siblings('.color-swatch').removeClass('active')
      if(inventory_quantity > 0){
        $('.color-swatch-list .color-swatch__radio[name="option_{{ section.id | replace: "template--", ""  }}"]').removeAttr('checked');
        $(this).find('.color-swatch__radio[name="option_{{ section.id | replace: "template--", ""  }}"]').attr('checked', 'checked');
        // 修改标题
        colorBlock.find('.name lable').text($(this).find('.color-swatch__radio').val());
        // 图片跳转
        generateHtmlForVariant_{{se_id }}(vid);
        
        pid_{{ section.id | replace: "template--", ""  }} = vid;
        
      } else {
        // 没有库存
        // $(thatVariant).find('.product-tips').show();
        let oldName = colorBlock.find('.name lable').text();
        colorBlock.find('.name lable').text($(this).find('.color-swatch__radio').val());
        $('.color-swatch-list .color-swatch__radio[name="option1"]').removeAttr('checked');
        $(this).find('.color-swatch__radio[name="option1"]').attr('checked', 'checked');
        galleryTop_{{ section.id | replace: "template--", ""  }}.slideTo(_position);
        setTimeout(function(){
          $(thatVariant).find('.product-tips').fadeOut();
          colorBlock.find('.name lable').text(oldName);
          $('.color-swatch-list .color-swatch__radio[name="option_{{ section.id | replace: "template--", ""  }}"]').removeAttr('checked');
          $('.color-swatch-list .color-swatch__radio[name="option_{{ section.id | replace: "template--", ""  }}"]').eq(0).attr('checked', 'checked');
          galleryTop_{{ section.id | replace: "template--", ""  }}.slideTo(0);
        }, 2000)
      }
    });

    {% comment %} //规格切换
    $('#shopify-section-{{ section.id }}').on('click', '.product-components .plug-list .item', function () {
      var list = $(this).closest('.plug-block');
      list.find('.item').removeClass('active');
      $(this).addClass('active');
      // 修改标题
      list.find('.name label').text($(this).attr('name'));
    }); {% endcomment %}

    // more payment
    $('#shopify-section-{{ section.id }}').on('click', '.product-components .more-payment', function () {
      // commonGtmEvent('A10_P_PayOpt', 'click', '');
      $(this).find('.payment-methods-list').toggleClass('active');
    });

    fixBottomBar();

    // 折叠
    const myCollswiper = new Swiper('.collapsible-swiper', {
      loop: true, // 循环模式选项
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
      lazy: {
        loadPrevNext: true, // 预加载前后两张图片
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
    });

    $('#shopify-section-{{ section.id }}').on('click', '.box-collapsible-bar .item .box-head', function () {
      var item = $(this).closest('.item');
      if($(item).find('.box-detail').is(":hidden")){
        $(this).find('.add').fadeTo( "slow", 0);
        $(this).find('.minus').fadeTo( "slow", 1);
      } else {
        $(this).find('.add').fadeTo( "slow", 1);
        $(this).find('.minus').fadeTo( "slow", 0);
      }
      $(item).find('.box-detail').slideToggle('slow');
    });

    {% if section.settings.show_time %}
      const countdownP = new Countdown("{{ section.settings.text5 }}", ".price-block-countdown",'loop');
    {% endif %}
  });

  {% comment %} 置底栏 {% endcomment %}
  function fixBottomBar() {
    var topBar = $("#top-bar");
    let barTop = $('#product-components').offset().top + 150;
    $(window).scroll(function() {
      if ($(this).scrollTop() > barTop) {
          topBar.addClass("toolbar-fixed");
      } else {
          topBar.removeClass("toolbar-fixed");
      }
    });
  }


  function convertShopifyImageUrl(url, size) {
    return url.replace(/(\.[^/.]+)$/, `_${size}$1`);
  }

  function generateHtmlForVariant_{{se_id }}(vid) {
    let currentVariant = !vid ? variantArr_{{se_id }}[0] : vid;
    if (vid) {
      currentVariant = variantArr_{{se_id }}.filter(item => item.vid == vid)[0];
    }
    const { position, max } = currentVariant;
  
    let _productHtml = '';
    let _productnavHtml = '';
    
    for (let i = position - 1; i < max; i++) {
      const ths = productMediaArr_{{ se_id }}[i];
      let imageUrl = ths.src;
      let itemMainHtml = '';
      ths.alt = ths.alt ? ths.alt : 'Ulike Product Image';
      // 第一个直接加载 eager
      let loadingAttr = i == position - 1 ? 'fetchpriority="high"' : 'loading="lazy"';
  
      if (ths.media_type == 'video') {
        imageUrl = ths.preview_image.src;
        itemMainHtml = generateVideoHtml(ths.sources);
      } else {
        itemMainHtml = `<img class="product__image" ${loadingAttr} src="${convertShopifyImageUrl(imageUrl, '870x')}"  alt="${ths.alt}" media_type="${ths.media_type}" >
        `
      }
      _productHtml += `
         <div class="swiper-slide">${itemMainHtml}</div>`;
    }
    setTimeout(function () {
      $('#myThumbs_{{ section.id }} .swiper-wrapper').html(_productHtml);
      $('#myGallery_{{ section.id }} .swiper-wrapper').html(_productHtml);
      initProductImages_{{se_id }}();
      // 3D 模型加载
      if (currentVariant.isOriginalPrice == 'true') {
        $('.product-components_{{ se_id }} .price-block .dis-price').html(currentVariant.price);
        $('.product-components_{{ se_id }} .price-block .original-price').html(currentVariant.original_price);
      }else {
        if(currentVariant.price != '' && currentVariant.page_price != ''){
          $('.product-components_{{ se_id }} .price-block .dis-price').html(formatMoney(currentVariant.page_price));
          $('.product-components_{{ se_id }} .price-block .original-price').html(currentVariant.price);
        } else if(currentVariant.price != '') {
          $('.product-components_{{ se_id }} .price-block .dis-price').html(currentVariant.price);
          $('.product-components_{{ se_id }} .price-block .original-price').html('');
        }
      }

      if(currentVariant.variantTitle != ''){
        $('.product-components_{{ se_id }} .title').html(currentVariant.variantTitle);
      }

    }, 10)
  
  }
  
</script>

