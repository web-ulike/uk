  .product-components .product-left-panel .main-image-swiper .icon-img-top::before {
    position: absolute;
    content: '';
    display: none;
    width: 200%;
    height: 200%;
    z-index: 2;
    background: linear-gradient(
      -45deg,
      rgba(255, 255, 255, 0) 40%,
      rgba(255, 255, 255, 0.5) 50%,
      rgba(255, 255, 255, 0) 60%
    );
    animation: scow 3s linear infinite;
  }

