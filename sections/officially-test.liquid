<style>
  .officially-test{
  padding: 70px 0;
  background: #FBFAF4;
}

.officially-test .officially-test-flex{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.officially-test .officially-test-left{
  width: 58.88%;
  position: relative;
}

.officially-test .officially-test-right{
  width: 32.77%;
}

.officially-test .img{
  width: 100%;
  position: relative;
  z-index:1;
}

.officially-test .officially-test-title{
  color: #3F3636;
  font-size: 50px;
  line-height: 1.2;
  max-width: 610px;
}

.officially-test .officially-test-sub{
  color: #3F3636;
  font-size: 18px;
  line-height: 1.5;
}

.officially-test .officially-test-mt{
  margin-top: 60px;
}

.officially-test .officially-test-btn{
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  width: 280px;
  height: 45px;
  border-radius: 45px;
  border: 1px solid #3F3636;
  color: #3F3636;
  font-size: 16px;
  line-height: 1.5;
}

.officially-test-popup{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 999;
  background: rgba(0, 0, 0, 0);
  display: flex;
  justify-content: flex-end;
  pointer-events: none;
  transition: all .4s ease;
  overflow: hidden;
}
.officially-test-popup.active{
  background: rgba(0, 0, 0, .3);
  pointer-events: all;
}

.officially-test-popup .officially-test-popup-mark{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height:100%;
  z-index:1;
}

.officially-test-popup .officially-test-popup-cont{
  width: 100%;
  height: 100%;
  max-width: 1280px;
  background: #FBFAF4;
  padding: 78px;
  position: relative;
  z-index:2;
  transform: translateX(100%);
  transition: all .4s ease;
}

.officially-test-popup.active .officially-test-popup-cont{
  transform: translateX(0);
  transition: all .4s ease .1s;
}

.officially-test-popup .officially-test-popup-det{
  height: 100%;
  overflow-y: auto;
}
.officially-test-popup .officially-test-popup-det::-webkit-scrollbar {
    width: 10px;
}

.officially-test-popup .officially-test-popup-det::-webkit-scrollbar-track {
    background-color: none;
}

.officially-test-popup .officially-test-popup-det::-webkit-scrollbar-thumb {
    background-color: rgba(228, 221, 221, .3);
    border-radius: 5px;
}

.officially-test-popup .officially-test-popup-det::-moz-scrollbar {
  width: 10px;
}

.officially-test-popup .officially-test-popup-det::-moz-scrollbar-track {
  background: none;
}

.officially-test-popup .officially-test-popup-det::-moz-scrollbar-thumb {
  background: rgba(228, 221, 221, .3);
  border-radius: 5px;
}
.officially-test-popup .officially-test-popup-close{
  position: absolute;
  top: 80px;
  right: 70px;
  cursor: pointer;
  z-index: 3;
}

.officially-test-popup .officially-test-popup-title{
  color: #3F3636;
  font-size: 40px;
  line-height: 1.2;
}

.officially-test-popup .officially-test-popup-sub{
  color: #3F3636;
  font-size: 18px;
  line-height: 1.2;
  margin-top: 20px;
}

.officially-test-popup .officially-test-popup-select{
  margin-top: 45px;
}

.officially-test-popup .officially-test-popup-lebel{
  color: #190A0A;
  font-size: 20px;
  line-height: 1.2;
  display: flex;
  align-items: center;
  gap: 10px;
}

.officially-test-popup .officially-test-popup-lebel-span{
  width: 15px;
  height: 15px;
  border-radius: 0 0 10px 10px;
  border: 1px solid #000;
  background-size: cover;
}

.officially-test-popup .officially-test-popup-list{
  display: flex;
  gap: 15px;
  margin-top: 15px;
  overflow-x: auto;
}

.officially-test-popup .officially-test-popup-item{
  width: 115px;
  cursor: pointer;
  flex-shrink: 0;
}

.officially-test-popup .officially-test-popup-img{
  position: relative;
}

.officially-test-popup .officially-test-popup-img::before{
  position: absolute;
  content: '';
  width: 16px;
  height: 16px;
  border-radius: 50%;
  display: block;
  background: #fff;
  border: 4px solid #000;
  right: 4px;
  bottom: -4px;
  z-index: 2;
  opacity: 0;
  box-shadow: 0 0 0 4px #fff;
  transition: all .4s ease;
}
.officially-test-popup .officially-test-popup-item.active .officially-test-popup-img::before,
.officially-test-popup .officially-test-popup-item:hover .officially-test-popup-img::before{
  right: 8px;
  bottom: 4px;
  opacity: 1;
}
.officially-test-popup .img{
  width: 100%;
  position: relative;
  z-index: 1;
}

.officially-test-popup .officially-test-popup-p{
  color: #000;
  text-align: center;
  font-size: 16px;
  line-height: 1.5;
  margin-top: 8px;
}

.officially-test-popup-btn-box{
  margin-top: 45px;
  display: flex;
  gap: 20px;
}
.officially-test-popup .officially-test-popup-btn{
  width: 137px;
  height: 45px;
  border-radius: 45px;
  display: none;
  align-items: center;
  justify-content: center;
  gap: 10px;
  border: 1px solid #190A0A;
  color: #190A0A;
  font-size: 12px;
  line-height: 1.5;
  cursor: pointer;
}

.officially-test-popup .officially-test-popup-submit{
  opacity: .3;
  pointer-events: none;
  transition: all .4s ease;
  display: flex;
}

.officially-test-popup .officially-test-popup-btn.active{
  background: #190A0A;
  color: #fff;
  opacity: 1;
  pointer-events: all;
}

.officially-test-popup .officially-test-popup-btn.active path{
  fill:#fff;
}

.officially-test-popup .officially-test-popup-reset:not(.active){
  border: 0;
  width: auto;
}

.officially-test-popup .officially-test-popup-msg{
  color: #3F3636;
  font-size: 14px;
  line-height: 1.5;
  max-width: 588px;
  margin-top: 30px;
}

.officially-test-popup-results-item{
