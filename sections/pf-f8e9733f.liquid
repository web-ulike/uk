<script>
  document.querySelectorAll(".carousel-wrapper .carousel-item").forEach(function (el) {
    el.addEventListener("click", () => {
      // --- 添加调试日志 ---
      console.log("Carousel item clicked:", el);
      let name = el.getAttribute("data-name");
      console.log("Data-name found:", name);
      // --- 结束调试日志 ---

      if (name) {
        const popupElement = document.querySelector(`.${name}`);
        // --- 添加调试日志 ---
        console.log("Popup element found:", popupElement);
        // --- 结束调试日志 ---
        if (popupElement) { // 检查元素是否存在
           fadeIn(popupElement)
        } else {
           console.error("Popup element with class", name, "not found!");
        }
      }
    })
  })

  document.querySelectorAll(".place-image-popup").forEach(function (el) {
    el.addEventListener("click", () => {
      fadeOut(el)
    })
  })

  // dom显示
  function fadeIn(element, callback = null, duration = 400) {
    if (window.getComputedStyle(element).display === 'none') {
      element.style.display = 'block'; // 先显示元素
      element.style.opacity = 0; // 设为透明
    }

    let startTime = performance.now();

    function animate(currentTime) {
      let elapsedTime = currentTime - startTime;
      let progress = elapsedTime / duration;

      if (progress >= 1) {
        element.style.opacity = 1; // 透明度设为 1
        if (callback) callback(); // 调用回调函数
      } else {
        element.style.opacity = progress;
        requestAnimationFrame(animate);
      }
    }

    requestAnimationFrame(animate);
  }

  // dom隐藏
  function fadeOut(element, callback = null, duration = 400) {
    let startTime = performance.now();

    function animate(currentTime) {
      let elapsedTime = currentTime - startTime;
      let progress = elapsedTime / duration;

      if (progress >= 1) {
        element.style.opacity = 0;
        element.style.display = 'none'; // 完全隐藏
        if (callback) callback(); // 调用回调函数
      } else {
        element.style.opacity = 1 - progress;
        requestAnimationFrame(animate);
      }
    }

    requestAnimationFrame(animate);
  }

  (function () {
      const track = document.querySelector('.carousel-track');
      const allItems = Array.from(track.children);
      const originalLength = 5; // 实际可见的项目数量
      const defaultSideCount = 2; // 两侧的“幽灵”项目数量
      let currentIndex = defaultSideCount; // 从第一个真实项目开始
      let defNum = 0;
      let isTransitioning = false; // 添加一个标志来防止快速点击导致的问题
      const transitionDuration = 400; // 与 CSS 中的 transition 时间一致
      let autoPlayInterval = null; // 用于自动播放的计时器
      let autoPlayTimeout = null; // 用于暂停后恢复的计时器
      const autoPlayDelay = 3000; // 自动播放间隔，例如3秒

      const dotsContainer = document.querySelector('.carousel-dots');

      function getSideCount() {
        // 注意：这里的 sideCount 实际上是指实际项目区域开始的索引偏移
        // 如果你的幽灵元素数量固定为2，可以直接返回2
        // 如果需要动态计算，则需要根据视口调整
        const wrapperWidth = track.parentElement.offsetWidth;
        const isMobile = wrapperWidth <= 798;
        // 假设无论移动端还是桌面端，两侧都固定复制2个元素用于循环
        return 2; // 固定为幽灵元素的数量
      }

      function renderDots(sideCount) {
        dotsContainer.innerHTML = '';
        // 计算当前应该激活的点的索引，考虑循环
        // (currentIndex - sideCount) 是当前真实项目在 0-based 索引中的位置
        // + originalLength 再 % originalLength 是为了处理负数取模的情况，确保索引在 0 到 originalLength-1 之间
        const activeDotIndex = (currentIndex - sideCount + originalLength) % originalLength;
        for (let i = 0; i < originalLength; i++) {
          const dot = document.createElement('span');
          dot.className = 'carousel-dot' + (activeDotIndex === i ? ' active' : '');
          dot.onclick = () => {
            if (isTransitioning) return; // 防止过渡期间点击
            // 计算目标 currentIndex
            // 需要从 dot 的索引 i 转换回 allItems 中的 currentIndex
            currentIndex = i + sideCount;
            update(true); // 跳转到指定点对应的项目
          };
          dotsContainer.appendChild(dot);
        }
      }

      function update(isNoFirst = false, skipTransition = false) {
        isTransitioning = !skipTransition; // 标记开始/结束过渡
        const wrapperWidth = track.parentElement.offsetWidth;
        const isMobile = wrapperWidth <= 798;
        const baseSize = isMobile
          ? Math.floor(wrapperWidth * 0.64)
          : Math.floor(wrapperWidth * 0.293);
        const margin = isMobile ? 6 : 15;

        const sideCount = getSideCount(); // 获取幽灵元素的数量

        allItems.forEach((el, idx) => {
          el.classList.remove('active');
          // 使用 (idx - sideCount + originalLength) % originalLength 来判断是否是当前激活的 "真实" 项目对应的索引
          const realIndex = (idx - sideCount + originalLength) % originalLength;
          const activeRealIndex = (currentIndex - sideCount + originalLength) % originalLength;

          // 计算相对于当前中心项的距离，考虑循环
          let distance = idx - currentIndex;

          // --- 调整 active 状态和尺寸计算逻辑以适应循环 ---
          // 找到视觉上中间的项目索引
          const visualCenterIndex = currentIndex;

          // 重新计算距离，以视觉中心为基准
          distance = idx - visualCenterIndex;

          let width;
          // 激活状态给视觉中心项
          if (idx === visualCenterIndex) {
            width = Math.floor(baseSize * 1);
            el.classList.add('active');
            el.style.height = isMobile
              ? `${width * 1.16}px`
              : `${width * 1.164}px`;
          } else if (Math.abs(distance) === 1) { // 相邻项
            width = Math.floor(baseSize * (isMobile ? 0.54 : 0.569));
            el.style.height = isMobile
              ? `${width * 1.9}px`
              : `${width * 1.9}px`;
          } else { // 更远项
            width = Math.floor(baseSize * (isMobile ? 0.54 : 0.56));
            el.style.height = isMobile
              ? `${width * 1.9}px`
              : `${width * 1.83}px`;
          }
          el.style.width = `${width}px`;
          el.style.margin = `0 ${margin}px`;
        });

        // --- 调整 transform 计算 ---
        // 计算移动距离，始终以 currentIndex 为中心计算偏移量
        const currentItemElement = allItems[currentIndex];
        if (!currentItemElement) {
          console.error("Cannot find item at currentIndex:", currentIndex);
          return;
        }
        const itemStyle = window.getComputedStyle(currentItemElement);
        const itemWidth = currentItemElement.offsetWidth;
        const itemMargin = parseFloat(itemStyle.marginLeft) + parseFloat(itemStyle.marginRight);
        const itemTotalWidth = itemWidth + itemMargin;

        // 计算从 track 开头到 currentIndex 中心点的总宽度
        let totalWidthBeforeCenter = 0;
        for (let i = 0; i < currentIndex; i++) {
          const item = allItems[i];
          const style = window.getComputedStyle(item);
          totalWidthBeforeCenter += item.offsetWidth + parseFloat(style.marginLeft) + parseFloat(style.marginRight);
        }
        const currentItemCenterOffset = totalWidthBeforeCenter + (itemWidth / 2) + parseFloat(itemStyle.marginLeft);

        // 计算容器中心点
        const wrapperCenter = track.parentElement.offsetWidth / 2;

        // 计算需要移动的距离
        let move = wrapperCenter - currentItemCenterOffset;

        // 应用 transform
        track.style.transition = skipTransition ? 'none' : `transform ${transitionDuration / 1000}s ease`;
        track.style.transform = `translateX(${move}px)`;


        // 更新 defNum (如果还需要的话，但基于中心对齐的计算可能不再需要它)
        // defNum = move; // 如果后续逻辑依赖 defNum，则更新

        renderDots(sideCount); // 更新指示点

        // 如果不是跳跃（即有动画），则在动画结束后解除锁定
        if (!skipTransition) {
          setTimeout(() => {
            isTransitioning = false;
          }, transitionDuration);
        } else {
          isTransitioning = false; // 跳跃是瞬时的
        }
      }

      // --- 修改 prev 和 next 实现循环 ---
      function prev() {
        if (isTransitioning) return;
        const sideCount = getSideCount();
        currentIndex--;
        update(true); // 先移动到目标位置（可能是幽灵元素）

        // 检查是否移动到了开头的幽灵区域
        if (currentIndex < sideCount) {
          isTransitioning = true; // 标记开始特殊过渡（跳跃）
          // 等待动画完成
          setTimeout(() => {
            // 无动画地跳到末尾对应的真实元素
            currentIndex = sideCount + originalLength - 1;
            update(true, true); // 使用 skipTransition=true 更新位置
          }, transitionDuration);
        }
      }

      function next() {
        if (isTransitioning) return;
        const sideCount = getSideCount();
        currentIndex++;
        update(true); // 先移动到目标位置（可能是幽灵元素）

        // 检查是否移动到了末尾的幽灵区域
        if (currentIndex >= sideCount + originalLength) {
          isTransitioning = true; // 标记开始特殊过渡（跳跃）
          // 等待动画完成
          setTimeout(() => {
            // 无动画地跳到开头对应的真实元素
            currentIndex = sideCount;
            update(true, true); // 使用 skipTransition=true 更新位置
          }, transitionDuration);
        }
      }


      // --- 初始化和事件绑定 ---
      window.addEventListener('resize', () => update()); // 尺寸变化时重新计算
      window.addEventListener('load', () => {
        // 初始计算位置，确保居中对齐第一个真实元素
        update(false); // 首次加载计算初始位置
      });


      let startX = 0;
      let startY = 0; // 新增：记录起始 Y 坐标
      let isDragging = false;
      let lastTransform = 0;
      let isDraggingThresholdMet = false;
      const dragThreshold = 10;
      let isScrollingHorizontally = false; // 新增：标记是否确定为水平滑动
      let isDirectionLocked = false; // 新增：标记滑动方向是否已确定

      function getCurrentTransform() {
        const style = window.getComputedStyle(track);
        const matrix = style.transform;
        if (matrix && matrix !== 'none') {
          const match = matrix.match(/matrix.*\((.+)\)/);
          if (match) {
            const values = match[1].split(', ');
            return parseFloat(values[4]); // 获取 translateX 值
          }
        }
        return 0;
      }

      track.addEventListener('touchstart', (e) => {
        if (isTransitioning) return; // 过渡期间不允许拖动
        isDragging = true;
        startX = e.touches[0].clientX;
        startY = e.touches[0].clientY; // 记录起始 Y 坐标
        lastTransform = getCurrentTransform(); // 获取当前的 transform 值
        track.style.transition = 'none'; // 拖动时移除动画
        isDraggingThresholdMet = false;
        isScrollingHorizontally = false; // 重置水平滑动标记
        isDirectionLocked = false; // 重置方向锁定标记
        // clearAutoPlay(); // 用户开始交互时暂停自动播放
      }, { passive: true }); // touchstart 可以是 passive

      window.addEventListener('touchmove', (e) => {
        if (!isDragging) return;

        const currentX = e.touches[0].clientX;
        const currentY = e.touches[0].clientY;
        const dx = currentX - startX;
        const dy = currentY - startY;

        if (!isDirectionLocked) {
          // 首次移动或方向未确定时，判断主要滑动方向
          if (Math.abs(dx) > dragThreshold || Math.abs(dy) > dragThreshold) {
            if (Math.abs(dx) > Math.abs(dy)) {
              // 水平滑动为主
              isScrollingHorizontally = true;
            }
            // else { // 垂直滑动为主，isScrollingHorizontally 保持 false }
            isDirectionLocked = true; // 标记方向已确定
          }
        }

        if (isScrollingHorizontally) {
          // 如果确定是水平滑动，阻止页面垂直滚动
          e.preventDefault();
          // 直接在 lastTransform 基础上应用拖动距离
          track.style.transform = `translateX(${lastTransform + dx}px)`;
          if (Math.abs(dx) > dragThreshold) { // 检查是否超过水平拖拽阈值
            isDraggingThresholdMet = true;
          }
        }
        // 如果是垂直滑动 (isScrollingHorizontally is false)，则不执行 preventDefault 和 transform 更新
        // 让浏览器处理垂直滚动

      }, { passive: false }); // !! 必须移除 passive: true 或设为 false 才能调用 preventDefault

      function endDrag(dx) {
        const threshold = 60; // 滑动切换的阈值
        if (dx > threshold) {
          prev(); // 调用 prev 实现循环切换
        } else if (dx < -threshold) {
          next(); // 调用 next 实现循环切换
        } else {
          // 如果滑动距离不足以切换，恢复到当前 currentIndex 的位置
          update(true); // 使用动画恢复
        }
      }

      window.addEventListener('touchend', (e) => {
        if (!isDragging) return;
        const dx = e.changedTouches[0].clientX - startX;
        isDragging = false;

        if (isScrollingHorizontally) { // 仅当之前是水平滑动时才处理轮播图逻辑
          if (isDraggingThresholdMet) {
            endDrag(dx); // 执行切换或恢复逻辑
          } else {
            // 如果未达到拖拽阈值，视为点击，查找被点击的元素
            const clickedElement = e.target.closest('.carousel-item');
