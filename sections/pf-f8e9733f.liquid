  <title>Marquee Slider</title>
  <style>
    .marquee {
      overflow: hidden;
      position: relative;
      width: 100%;
      padding: 40px 0;
      background: #fff;
      white-space: nowrap;
    }

    .marquee .slider {
      display: flex;
      gap: 30px;
      animation: marqueeScroll 15s linear infinite;
    }

    .marquee .slider img {
      max-height: 70px;
      display: block;
    }

    @keyframes marqueeScroll {
      from {
        transform: translateX(0);
      }

      to {
        transform: translateX(-100%);
      }
    }

    .marquee:hover .slider {
      animation-play-state: paused;
    }

    @media (max-width: 768px) {
      .marquee .slider {
        display: flex;
        gap: 15px;
        animation: marqueeScroll 5s linear infinite;
      }

      .marquee .slider img {
        max-height: 30px;
        display: block;
      }
    }
  </style>
</head>

