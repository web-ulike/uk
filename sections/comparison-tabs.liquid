<script>
  window.addEventListener('load', function() {
    function hairAdsTabs{{ section.id | replace: "_", "" | replace: "-", "" }}() {
      let $p = $('.hair-ads-tabs.tabs-{{ section.id }}');
      let $items = $p.find('.hair-ads-tabs-item');
      let $items1 = $p.find('.hair-ads-tabs-list .hair-ads-tabs-item');
      let $tap = $p.find('.hair-ads-tabs-list-item');
      let $imgs = $p.find('.hair-ads-tabs-img-list .img-box');
      let $txtP = $p.find('.hair-ads-tabs-right .hair-ads-tabs-p');
      $tap.on('click', function(e) {
        setTab($(this).index())
      })

      function setTab(index) {
        if(index!=0){
          $p.addClass('radius-left')
        }else{
          $p.removeClass('radius-left')
        }
        $tap.eq(index).addClass('active').siblings().removeClass('active');
        $imgs.eq(index).addClass('active').siblings().removeClass('active');
        $items.eq(index).addClass('active').siblings().removeClass('active');
        $items1.eq(index).addClass('active').siblings().removeClass('active');
      }
    }
    hairAdsTabs{{ section.id | replace: "_", "" | replace: "-", ""}}()
  })
</script>
