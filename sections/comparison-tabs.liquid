<style>
.hair-ads-tabs{
  padding: 82px 0;
}

.hair-ads-tabs .hair-ads-tabs-top{
  display: flex;
  justify-content: space-between;
  gap: 10px;
  color: #3F3636;
}

.hair-ads-tabs .hair-ads-tabs-title{
  font-size: 50px;
  line-height: 1.2;
  letter-spacing: -0.26px;
  max-width: 760px;
}

.hair-ads-tabs .hair-ads-tabs-sub{
  font-size: 18px;
  line-height: 1.5;
  max-width: 560px;
}

.hair-ads-tabs .hair-ads-tabs-list-top{
  margin-top: 54px;
  display: flex;
  gap: 12px;

}
.hair-ads-tabs .hair-ads-tabs-list-item{
  display: flex;
  align-items: center;
  padding: 0 70px;
  height: 67px;
  color: #3F3636;
  font-size: 24px;
  line-height: 1.4;
  background: #F8F8F8;
  position: relative;
  z-index: 1;
  border-radius: 16px 16px 0 0;
  border: 1.68px solid #E4DDDD;
  border-bottom: 0;
  cursor: pointer;
  padding-top: 8px;
}

.hair-ads-tabs .hair-ads-tabs-list-item.active{
  color: #E0004D;
  font-family: var(--heading-font-family);
  background: #FBFAF4;
  z-index: 3;
}

.hair-ads-tabs .hair-ads-tabs-cont{
  position: relative;
  top: -1.68px;
  z-index: 2;
  background: #FBFAF4;
  display: flex;
  gap: 20px;
  padding: 43px;
  border: 1.68px solid #E4DDDD;
  border-radius: 0 16px 16px 16px;
}

.radius-left.hair-ads-tabs .hair-ads-tabs-cont{
  border-radius: 16px;
}

.hair-ads-tabs .hair-ads-tabs-left,
.hair-ads-tabs .hair-ads-tabs-right{
  flex: 1;
}

.hair-ads-tabs .hair-ads-tabs-p{
  color: #3F3636;
  font-size: 28px;
  line-height: 1.5;
  border-bottom: 0.7px solid #3F3636;
  margin-top: 18px;
  display: flex;
  align-items: center;
  gap: 10px;
  justify-content: space-between;
  padding-bottom: 12px;
  position: relative;
  --left-num:0;
  --width-num:0;
}

.hair-ads-tabs .hair-ads-tabs-p-item{
  cursor: pointer;
}

.hair-ads-tabs .hair-ads-tabs-left .hair-ads-tabs-p{
  color: #E0004D;
}

.hair-ads-tabs .hair-ads-tabs-flex-m{
  display: flex;
  flex-direction: column-reverse;
  margin-bottom: 12px;
}

.hair-ads-tabs .hair-ads-tabs-text-p{
  color: #3F3636;
  font-size: 24px;
  line-height: 1;
  letter-spacing: 0.24px;
  display: flex;
  align-items: center;
  gap: 25px;
  padding: 17px 0;
  border-bottom: 0.7px solid #E4DDDD;
  
}

  .hair-ads-tabs .hair-ads-tabs-text-p svg{
    flex-shrink: 0;
  }

.hair-ads-tabs .img{
  width: 100%;
}

.hair-ads-tabs .mb-box{
  display: none;
}

.hair-ads-tabs .hair-ads-tabs-img-list .img-box{
  display: none;
}

.hair-ads-tabs .hair-ads-tabs-img-list .active{
  display: block;
}

.hair-ads-tabs .hair-ads-tabs-item{
  display: none;
}

.hair-ads-tabs .hair-ads-tabs-item.active{
  display: block;
}

.hair-ads-tabs .hair-ads-tabs-p-span{
  position: absolute;
  height: 4px;
  width: var(--width-num);
  left: var(--left-num);
  bottom: 0;
  background: #3F3636;
  transition: all .4s ease;
}

.hair-ads-tabs .svg-icon{
  position: absolute;
  bottom: 0.3px;
  width: 16px;
  height: 16px;
  overflow: hidden;
  pointer-events: none;
}
.hair-ads-tabs .svg-left{
  transform: scale(-1,1);
  left: -16px;
}
.hair-ads-tabs .svg-right{
  right: -16px;
}
.hair-ads-tabs .hair-ads-tabs-list-item:first-child .svg-left{
  opacity: 0;
}
.hair-ads-tabs .hair-ads-tabs-list-item:first-child::before{
  content: '';
  position: absolute;
  width: 15px;
  height: 50px;
  background: #F8F8F8;
  pointer-events: none;
  left: -1.6px;
  border-left: 1.68px solid #E4DDDD;
  bottom: -30px;
  z-index: 1;
}

.hair-ads-tabs .hair-ads-tabs-list-item.active::before{
  display: none;
}

@media (max-width:1600px) {
  .hair-ads-tabs .hair-ads-tabs-p{
    font-size: 20px;
  }
  .hair-ads-tabs .hair-ads-tabs-text-p{
    font-size: 20px;
  }
  .hair-ads-tabs .hair-ads-tabs-list-item{
    font-size: 18px;
    height: 55px;
    padding: 0 40px;
  }
}

@media (max-width:1280px) {
  .hair-ads-tabs .hair-ads-tabs-p{
    font-size: 16px;
  }
  .hair-ads-tabs .hair-ads-tabs-text-p{
    font-size: 14px;
    gap: 15px;
  }
  .hair-ads-tabs .hair-ads-tabs-text-p svg{
    zoom: .7;
  }
  .hair-ads-tabs .hair-ads-tabs-p-span{
    height: 2px;
  }
}

@media (max-width:991px) {
  .hair-ads-tabs{
    padding: 50px 0;
  }
  .hair-ads-tabs .pc-box{
    display: none;
  }
  .hair-ads-tabs .mb-box{
    display: block;
  }
  .hair-ads-tabs .hair-ads-tabs-title{
    max-width: none;
    font-size: 24px;
  }
  .hair-ads-tabs .hair-ads-tabs-sub{
    font-size: 14px;
  }
  .hair-ads-tabs .hair-ads-tabs-top{
    flex-direction: column;
  }
  .hair-ads-tabs .hair-ads-tabs-cont{
    gap: 11px;
    padding: 18px;
    border-radius: 0 10px 10px 10px;
  }
  .radius-left.hair-ads-tabs .hair-ads-tabs-cont{
    border-radius: 10px;
  }
  .hair-ads-tabs .hair-ads-tabs-flex-m{
    margin-bottom: 0;
  }
  .hair-ads-tabs .hair-ads-tabs-p{
    margin-bottom: 0;
    margin-top: 7px;
    padding-bottom: 12px;
    font-size: 14px;
    border: 0;
  }
  .hair-ads-tabs .hair-ads-tabs-p-span{
    display: none;
  }

  .hair-ads-tabs .hair-ads-tabs-text-p{
    font-size: 12px;
    gap: 14px;
    position: relative;
    min-height: 49px;
  }
  .hair-ads-tabs .hair-ads-tabs-text-p svg{
    zoom: .6;
  }
  .hair-ads-tabs .hair-ads-tabs-left .hair-ads-tabs-text-p::before{
    position: absolute;
    content: '';
    display: block;
    width: calc(200% + 11px);
    height: 0.7px;
    background: #E4DDDD;
    left:0;
    bottom: -0.7px;
  }
  .hair-ads-tabs .hair-ads-tabs-text{
    position: relative;
  }
  .hair-ads-tabs .hair-ads-tabs-left .hair-ads-tabs-text::before{
    position: absolute;
    content: '';
    display: block;
    width: calc(200% + 11px);
    height: 0.7px;
    background: #E4DDDD;
    left:0;
    top: 0;
  }
  .hair-ads-tabs .hair-ads-tabs-text-p{
    padding: 12px 0;
    border: 0;
  }
  .hair-ads-tabs .hair-ads-tabs-list-top{
    gap: 7px;
    margin-top: 40px;
  }
  .hair-ads-tabs .hair-ads-tabs-list-item{
    height: 34px;
    font-size: 12px;
    padding: 0 20px;
    padding-top: 5px;
    border-radius: 10px 10px 0 0;
  }
  .hair-ads-tabs .svg-icon{
    width: 10px;
    height: 10px;
  }
  .hair-ads-tabs .svg-left{
    left: -10px;
  }
  .hair-ads-tabs .svg-right{
    right: -10px;
  }
}
</style>
