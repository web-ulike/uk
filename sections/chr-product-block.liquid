  function generateGiftHtml(images){
    let _productHtml = '';
    for (let i = 0; i < images.length; i++) {
      let imageUrl = images[i];
      let itemMainHtml = '';
      let alt = 'Ulike Product Image';
      // 第一个直接加载 eager
      let loadingAttr = 'loading="lazy"';
      itemMainHtml = `<img class="product__image" ${loadingAttr} src="${imageUrl}"  alt="${alt}" >`;
      _productHtml += `<div class="swiper-slide">${itemMainHtml}</div>`;
    }
    setTimeout(function(){
      $('#myThumbs_{{ section.id }} .swiper-wrapper').html(_productHtml);
      $('#myGallery_{{ section.id }} .swiper-wrapper').html(_productHtml);
      initProductImages_{{ se_id }}();
    }, 100)
  }

  {% comment %} 置底栏 {% endcomment %}
  function fixBottomBar() {
    var topBar = $("#top-bar");
    let barTop = $('#product-components').offset().top + 150;
    $(window).scroll(function() {
      if ($(this).scrollTop() > barTop) {
          topBar.addClass("toolbar-fixed");
      } else {
          topBar.removeClass("toolbar-fixed");
      }
    });
  }

  function initProductImages_{{ se_id }}() {
    if (galleryThumbs_{{ section.id | replace: "template--", ""  }} !== null) {
      galleryThumbs_{{ section.id | replace: "template--", ""  }}.destroy(true, true);
    }
    galleryThumbs_{{ section.id | replace: "template--", ""  }} = new Swiper('#myThumbs_{{ section.id }}', {
      spaceBetween: isMobileW() ? 8 : 15,
      slidesPerView: 'auto',
      lazy: {
        loadPrevNext: true, // 预加载前后两张图片
      },
      freeMode: true,
      watchSlidesVisibility: true,
      watchSlidesProgress: true,
      direction: isMobileW()? 'horizontal' : 'vertical'
      {% comment %} breakpoints: {
        640: {
          direction: 'horizontal', // 小屏幕上水平方向滚动
        },
        1024: {
          direction: 'vertical', // 大屏幕上垂直方向滚动
        },
      } {% endcomment %}
    });
    if (galleryTop_{{ section.id | replace: "template--", ""  }} !== null) {
      galleryTop_{{ section.id | replace: "template--", ""  }}.destroy(true, true);
    }
    galleryTop_{{ section.id | replace: "template--", ""  }} = new Swiper('#myGallery_{{ section.id }}', {
      lazy: {
        loadPrevNext: true, // 预加载前后两张图片
      },
      spaceBetween: 10,
      loop: true,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      thumbs: {
        swiper: galleryThumbs_{{ section.id | replace: "template--", ""  }},
      }
    });
  }

  function generateHtmlForVariant_{{ se_id }}(vid) {
    let currentVariant = !vid ? variantArr_{{ se_id }}[0] : vid;
    if (vid) {
      currentVariant = variantArr_{{ se_id }}.filter(item => item.vid == vid)[0];
    }
    const { position, max } = currentVariant;
  
    let _productHtml = '';
    let _productnavHtml = '';
    
    for (let i = position - 1; i < max; i++) {
      const ths = productMediaArr_{{ se_id }}[i];
      let imageUrl = ths.src;
      let itemMainHtml = '';
      ths.alt = ths.alt ? ths.alt : 'Ulike Product Image';
      // 第一个直接加载 eager
      let loadingAttr = i == position - 1 ? 'fetchpriority="high"' : 'loading="lazy"';
      const pictureElement = `
        <picture>
            <source media="(max-width: 768px)" srcset="${convertShopifyImageUrl(imageUrl, '800x')}">
            <img class="product__media-image" ${loadingAttr} src="${convertShopifyImageUrl(imageUrl, '1740x')}" alt="${ths.alt}" media_type="${ths.media_type}" >
        </picture>
        `;

      if (ths.media_type == 'video') {
        imageUrl = ths.preview_image.src;
        itemMainHtml = generateVideoHtml(ths.sources);
      } else {
        itemMainHtml = pictureElement;
      }
      _productHtml += `
         <div class="swiper-slide">${itemMainHtml}</div>`;
    }
    setTimeout(function () {
      $('#myThumbs_{{ section.id }} .swiper-wrapper').html(_productHtml);
      $('#myGallery_{{ section.id }} .swiper-wrapper').html(_productHtml);
      initProductImages_{{ se_id }}();
      // 3D 模型加载
      if(currentVariant.price != '' && currentVariant.page_price != ''){
        $('.product-components_{{ se_id }} .price-block .dis-price').html(formatMoney(currentVariant.page_price));
        $('.product-components_{{ se_id }} .price-block .original-price').html(currentVariant.price);
      } else if(currentVariant.price != '') {
        $('.product-components_{{ se_id }} .price-block .dis-price').html(currentVariant.price);
        $('.product-components_{{ se_id }} .price-block .original-price').html('');
      }
      if(currentVariant.variantTitle != ''){
        $('.product-components_{{ se_id }} .title').html(currentVariant.variantTitle);
      }
    }, 10)
  }


  function convertShopifyImageUrl(url, size) {
    return url.replace(/(\.[^/.]+)$/, `_${size}$1`);
  }


