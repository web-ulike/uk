<script>
  // 定义初始化变量
  var variantArr{{ section.id | replace: "template--", "" | replace: "__", ""  }} = [];
  {% for variant in product.variants %}
      variantArr{{ section.id | replace: "template--", "" | replace: "__", ""  }}.push({
        variant_title:'{{ variant.metafields.custom["variant_title"] }}',
        page_price:'{{ variant.metafields.custom["page_price"] }}',
        price: '{{ variant.price | money }}'
      });
  {% endfor %}

  function listScroll{{ section.id | replace: "template--", "" | replace: "__", ""  }}(){
     const chrContainer = document.querySelector('#shopify-section-{{ section.id }} .gift-new-box-list');
      let isDown = false;
      let startX;
      let scrollLeft;
      if (chrContainer){
        chrContainer.addEventListener('mousedown', (e) => {
          isDown = true;
          startX = e.pageX - chrContainer.offsetLeft;
          scrollLeft = chrContainer.scrollLeft;
        });

        chrContainer.addEventListener('mouseleave', () => {
          isDown = false;
        });

        chrContainer.addEventListener('mouseup', () => {
          isDown = false;
        });

        chrContainer.addEventListener('mousemove', (e) => {
          if (!isDown) return;
          e.preventDefault();
          const x = e.pageX - chrContainer.offsetLeft;
          const walk = (x - startX) * 2; // 调整滑动速度
          chrContainer.scrollLeft = scrollLeft - walk;
        });
      }
  }
  listScroll{{ section.id | replace: "template--", "" | replace: "__", ""  }}()
</script>
