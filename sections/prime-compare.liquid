          <div class="prime-compare-msg">
            {{ section.settings.text5 }}
          </div>
        </div>
      </div>
    </div>
  </section>
  <script>
    window.addEventListener('load', function () {
      function changeCompare() {
        $p = $('.prime-compare')
        $p.find('.popup-tap span').text($p.find('.prime-compare-popup-item:first').text())
        $p.find('.popup-tap').click(function () {
          $(this).siblings('.prime-compare-popup').slideToggle()
        })
        $p.find('.prime-compare-popup-item').click(function () {
          let $this = $(this)
          let index = $this.index()
          $this.addClass('active').siblings().removeClass('active')
          $p.find('.popup-tap span').text($this.text())
          $p.find('.img-list img').removeClass('active').eq(index).addClass('active')
          $p.find('.prime-compare-list').each(function () {
            $(this).find('.prime-compare-list-item').removeClass('active').eq(index).addClass('active')
          })
          $this.parent().slideUp()
        })
      }
      changeCompare()
    })
  </script>
