<style>
.prime-compare {
  padding: 90px 0;
  background: linear-gradient(180deg, #f0efff 6.66%, #fff 96.76%);
}

.prime-compare .prime-compare-title {
  color: #190a0a;
  font-size: 48px;
  line-height: 1.25;
  text-align: center;
}

.prime-compare .prime-compare-cont {
  margin: 0 auto;
  margin-top: 70px;
  max-width: 1042px;
}

.prime-compare .prime-compare-table {
  width: 100%;
  text-align: center;
  table-layout: fixed;
}
.prime-compare .prime-compare-table tr {
  border-bottom: 1px solid #d9d9d9;
}
.prime-compare .prime-compare-table td {
  padding: 24px 15px;
  position: relative;
}
.prime-compare .prime-compare-table .img {
  display: none;
  width: 113px;
  margin: 0 auto;
}

.prime-compare .prime-compare-table .active {
  display: block;
}

.prime-compare .prime-compare-top {
  color: #190a0a;
  font-size: 28px;
  line-height: 1.25;
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 5px;
  margin-top: 17px;
}

.prime-compare .prime-compare-red {
  color: #e0004d;
}
.prime-compare .popup-tap {
  cursor: pointer;
}
.prime-compare .prime-compare-popup {
  display: none;
  padding: 26px 30px;
  position: absolute;
  background: #fff;
  left: 50%;
  transform: translateX(-50%);
  z-index: 3;
  margin-top: 15px;
  border-radius: 10px;
  background: #fff;
  box-shadow: 0px 3px 14.1px 0px #d1d2e3;
}
.prime-compare .prime-compare-popup-item {
  color: #3f3636;
  font-size: 28px;
  line-height: 1;
  cursor: pointer;
}
.prime-compare .prime-compare-popup-item:not(:last-child) {
  margin-bottom: 25px;
}
.prime-compare .prime-compare-popup-item:hover {
  color: #000;
}
.prime-compare .prime-compare-popup-item.active {
  color: #000;
  text-decoration: underline;
}
.prime-compare .prime-compare-txt {
  color: #190a0a;
  font-size: 18px;
  line-height: 1.4;
  font-family: var(--heading-font-family);
}

.prime-compare .prime-compare-list-item {
  display: none;
  color: #190a0a;
  font-size: 20px;
}

.prime-compare .prime-compare-list-item.active {
  display: block;
}

.prime-compare .prime-compare-msg {
  color: #3f3636;
  font-size: 12px;
  line-height: 1.5;
  opacity: 0.65;
  margin-top: 30px;
}

.prime-compare .prime-compare-icon {
  font-size: 22px;
}

.prime-compare .prime-compare-icon.prime-compare-red {
  font-family: var(--heading-font-family);
}

@media (max-width: 991px) {
  .prime-compare .prime-compare-title {
    font-size: 24px;
  }
  .prime-compare .prime-compare-cont {
    margin-top: 35px;
  }
  .prime-compare .prime-compare-table td {
    padding: 18px 15px;
  }
  .prime-compare .prime-compare-table td svg {
    width: 22px;
  }
  .prime-compare .prime-compare-table .img {
    width: 70px;
  }
  .prime-compare .prime-compare-txt {
    font-size: 14px;
    line-height: 1.2;
  }
  .prime-compare .prime-compare-top {
    font-size: 18px;
    margin-top: 10px;
  }
  .prime-compare .prime-compare-list-item,
  .prime-compare .prime-compare-icon {
    font-size: 14px;
    line-height: 1.2;
  }
  .prime-compare .prime-compare-msg {
    margin-top: 16px;
  }
  .prime-compare .prime-compare-popup {
    padding: 20px 18px;
  }
  .prime-compare .prime-compare-popup-item{
    font-size: 19px;
  }
  .prime-compare .prime-compare-popup-item:not(:last-child) {
    margin-bottom: 15px;
  }
  
}

@media (max-width: 640px) {
  .prime-compare .prime-compare-table {
    width: calc(100% + 48px);
    margin-left: -24px;
  }
}
