<style>
.officially-product{
  position: relative;
  height: 410vh;
}

.officially-product .officially-product-header{
  padding: 105px 0 95px 0;
  text-align: center;
  color: #190A0A;
}

.officially-product .officially-product-header-title{
  font-size: 50px;
  line-height: 1.2;
  max-width: 600px;
  margin: 0 auto;
}

.officially-product .officially-product-header-sub{
  font-size: 18px;
  line-height: 1.5;
  max-width: 861px;
  margin: 0 auto;
  margin-top: 20px;
}

.officially-product .officially-product-body{
  position: sticky;
  height: 100vh;
  top: 0;
  overflow: hidden;
}

.officially-product .officially-product-img{
  position: absolute;
  width: 48%;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  transform-origin: top center;
  z-index: 1;
  mix-blend-mode:screen;
}

.officially-product .officially-product-video{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 3;
  opacity: 0;
}
.officially-product .officially-product-video img{
  width:100%;
}

.officially-product .officially-product-light{
  position: absolute;
  width: 11.8%;
  left: 50%;
  top: 14.2%;
  transform: translateX(-50%);
  z-index: 2;
  opacity: 0;
}

.officially-product .officially-product-t{
  display: block;
  width: 100%;
  position: relative;
  z-index: 1;
}

.officially-product .officially-product-text{
  position: absolute;
  width: 385px;
  top: 25%;
  left: 66%;
  transform: translateY(50%);
  color: #3F3636;
  opacity: 0;
  display: none;
  z-index: 2;
}

.officially-product .officially-text-five{
  left: 20%;
}

.officially-product .officially-product-icon{
  display: block;
}

.officially-product .officially-product-title{
  font-size: 48px;
  line-height: 1.2;
  margin-top: 10px;
}

.officially-product .officially-product-sub{
  font-size: 18px;
  line-height: 1.2;
  margin: 10px 0;
}

.officially-product .officially-product-btn{
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  width: 215px;
  height: 45px;
  border-radius: 45px;
  border: 1px solid #3F3636;
  margin-top: 40px;
  color: #3F3636;
  font-size: 16px;
  line-height: 1.5;
}

.officially-product-popup{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 999;
  background: rgba(0, 0, 0, 0);
  display: flex;
  justify-content: flex-end;
  /* opacity: 0; */
  pointer-events: none;
  transition: all .4s ease;
  overflow: hidden;
}
.officially-product-popup.active{
  /* opacity: 1; */
  background: rgba(0, 0, 0, .3);
  pointer-events: all;
}
.officially-product-popup .officially-product-mark{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height:100%;
  z-index:1;
}
.officially-product-popup .officially-product-cont{
  width: 100%;
  height: 100%;
  max-width: 1280px;
  background: #FBFAF4;
  padding: 80px 15px 80px 60px;
  position: relative;
  z-index:2;
  transform: translateX(100%);
  transition: all .4s ease;
}

.officially-product-popup.active .officially-product-cont{
  transform: translateX(0);
  transition: all .4s ease .1s;
}

.officially-product-popup .officially-product-popup-det{
  height: 100%;
  overflow-y: auto;
  display: flex;
  gap: 145px;
  color: #3F3636;
  padding-right: 185px;
}

.officially-product-popup .officially-product-popup-det::-webkit-scrollbar {
    width: 10px;
}

.officially-product-popup .officially-product-popup-det::-webkit-scrollbar-track {
    background-color: none;
}

.officially-product-popup .officially-product-popup-det::-webkit-scrollbar-thumb {
    background-color: rgba(228, 221, 221, .3);
    border-radius: 5px;
}

.officially-product-popup .officially-product-popup-det::-moz-scrollbar {
  width: 10px;
}

.officially-product-popup .officially-product-popup-det::-moz-scrollbar-track {
  background: none;
}

.officially-product-popup .officially-product-popup-det::-moz-scrollbar-thumb {
  background: rgba(228, 221, 221, .3);
  border-radius: 5px;
}
.officially-product-popup .officially-product-close{
  position: absolute;
  top: 80px;
  right: 70px;
  cursor: pointer;
  z-index: 3;
}

.officially-product-popup .officially-product-popup-left{
  width: 115px;
  flex-shrink: 0;
}

.officially-product-popup .officially-product-popup-sub{
  font-size: 24px;
  line-height: 1.2;
}

.officially-product-popup .officially-product-popup-title{
  font-size: 40px;
  line-height: 1.2;
}

.officially-product-popup .officially-product-popup-p{
  font-size: 18px;
  line-height: 1.2;
  margin-top: 20px;
}

.officially-product-popup .officially-product-popup-video{
  position: relative;
  margin-top: 60px;
}

.officially-product-popup .officially-product-popup-video .img{
  display: block;
  width: 100%;
  position: relative;
  z-index: 1;
}

.officially-product-popup .product-video{
  position: absolute;
  width: 100%;
  height: 100%;
  object-fit: cover;
  top: 0;
  left: 0;
  z-index: 2;
}

.officially-product-popup .video-wrapper__play-button{
    left: 50%;
    top:50%;
    transform: translate(-50%,-50%);
  }

@media (max-width:1280px) {
  .officially-product-popup .officially-product-popup-det{
    gap: 60px;
    padding-right: 100px;
  }
  .officially-product-popup .officially-product-popup-title{
    font-size: 35px;
  }
  .officially-product-popup .officially-product-popup-p{
    font-size: 16px;
  }
}

@media (max-width:991px) {
  .officially-product-popup{
    display: flex;
    align-items: flex-end;
  }

  .officially-product-popup .officially-product-cont{
    padding: 50px 20px;
    height: auto;
    max-height: 100%;
    transform: translateY(100%);
  }

  .officially-product-popup.active .officially-product-cont{
    transform: translateY(0);
  }

  .officially-product-popup .officially-product-close{
    top: 18px;
    right: 18px;
  }

  .officially-product-popup .officially-product-popup-det{
    gap: 10px;
    padding-right: 0;
    flex-direction: column;
  }

  .officially-product-popup .officially-product-popup-title{
    font-size: 24px;
  }

  .officially-product-popup .officially-product-popup-p{
    font-size: 12px;
    margin-top: 10px;
  }

  .officially-product-popup .officially-product-popup-left{
    width: auto;
  }

  .officially-product-popup .officially-product-popup-sub{
    font-size: 14px;
  }

  .officially-product-popup .officially-product-popup-video{
    margin-top: 40px;
  }

}

@media (max-width:1250px) {
  .officially-product .officially-product-text{
    max-width: 300px;
  }
  .officially-product .officially-product-title{
    font-size: 30px;
  }
  .officially-product .officially-product-sub{
    font-size: 14px;
  }
  .officially-product .officially-product-icon{
    zoom: .9;
  }
}

@media (max-width:991px) {
  .officially-product .officially-product-header{
    padding: 50px 0 70px 0;
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
    z-index: 3;
  }
  .officially-product .officially-product-header-title{
    font-size: 24px;
  }

  .officially-product .officially-product-header-sub{
    font-size: 14px;
    margin-top: 10px;
  }
  .officially-product .officially-product-body{
    height: 100vh;
    top: 0;
    padding-top: 70%;
  }

  .officially-product .officially-product-img{
    width: 65.87%;
    position: relative;
  }

  .officially-product .officially-product-text{
    width: 100%;
    left: 0;
    top: 11vh;
    text-align: center;
    max-width: none;
    transform: translate(20%,0);
    padding: 0 24px;
  }

  .officially-product .officially-text-five{
    left: 0;
    top: auto;
    bottom: 15%;
  }

  .officially-product .officially-product-icon{
    margin: 0 auto;
  }

  .officially-product .officially-product-btn{
    margin: 0 auto;
    font-size: 12px;
    width: 171px;
    height: 42px;
  }
  
}
