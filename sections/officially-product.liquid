      .to(".light-two",{'opacity':'0',ease:'power2.inOut'},'<')
      .to(".officially-product-img",{'margin-top':pam.mt2,ease:'power2.inOut'},'<')
      .to(".officially-text-three",{'display':'block','transform':'translate('+ pam.translateX +','+ pam.translateY +')','opacity':'1',ease:'power2.inOut'})
      .to(".light-three",{'opacity':'1',ease:'power2.inOut'},'<')
      .to(".officially-text-three",{'display':'none','transform':'translate('+ pam.translateX1 +','+ pam.translateY1 +')','opacity':'0',ease:'power2.inOut'})
      .to(".light-three",{'opacity':'0',ease:'power2.inOut'},'<')
      .to(".officially-product-img",{'margin-top':pam.mt3,ease:'power2.inOut'},'<')
      .to(".officially-text-four",{'display':'block','transform':'translate('+ pam.translateX +','+ pam.translateY +')','opacity':'1',ease:'power2.inOut'})
      .to(".light-four",{'opacity':'1',ease:'power2.inOut'},'<')
      .to(".officially-text-four",{'display':'none','transform':'translate('+ pam.translateX1 +','+ pam.translateY1 +')','opacity':'0',ease:'power2.inOut'})
      .to(".light-four",{'opacity':'0',ease:'power2.inOut'},'<')
      .to(".officially-product-img",{'margin-top':pam.mt4,ease:'power2.inOut'},'<')
      .to(".officially-product-img",{'margin-left':pam.ml1,'margin-top':pam.mt5,ease:'power2.inOut'})
    })
    ScrollTrigger.create({
      trigger: ".officially-product",
      start: "top+=60% top",
      end: "+=50%",
      scrub: true,
      // markers:true,
      onUpdate(self){
          const video = document.querySelector('.officially-product-video')
        // if(isPlay||window.innerWidth>991){
          if(self.progress){
            video.style.setProperty('opacity','1')
          }else{
            video.style.setProperty('opacity','0')
          }
          try {
            // pv.currentTime = self.progress * pv.duration
           imgP.playImg(Math.round(self.progress*15))
          } catch (error) {
            console.log(error);
          }
        // }
      },
      animation:
      gsap.timeline()
      .to(".officially-text-five",{'display':'block','transform':'translate('+ pam.translateX +','+ pam.translateY +')','opacity':'1',ease:'power2.inOut'})
    })
  }
  window.addEventListener('load', () => {
    // if(window.innerWidth<991){
    //   $('.officially-product .officially-product-video.pc-box').remove()
    // }else{
    //   $('.officially-product .officially-product-video.mb-box').remove()
    // }
    productScrollTo()
    $('.officially-product-btn').click(function(){
      let $p =  $('.officially-product-popup')
      $p.addClass('active')
      if($p.find('.product-video .video-wrapper__play-button').length > 0){
        // $p.find('.product-video').get(0).play()
        $p.find('.product-video .video-wrapper__play-button').click()
        
      }
      $('body').css('overflow','hidden')
    })
    $('.officially-product-close,.officially-product-mark').click(function(){
      let $p =  $('.officially-product-popup')
      $p.removeClass('active')
      // if($p.find('.product-video').length > 0){
      //   $p.find('.product-video').get(0).pause()
      //   $p.find('.product-video').get(0).currentTime = 0
      // }
      $('body').css('overflow','')
    })
  })
