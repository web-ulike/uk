{
  "sections": {
    "blog-post-banner": {
      "type": "blog-post-banner",
      "settings": {
        "show_category": true,
        "blog_header_background": "#f5f5f5",
        "blog_header_text_color": "#282828"
      }
    },
    "main": {
      "type": "main-article",
      "settings": {
        "show_on_scroll_navigation": false,
        "show_author": false,
        "show_date": true,
        "show_share_buttons": true
      }
    },
    "blog-post-prev-next": {
      "type": "blog-post-prev-next",
      "settings": {}
    },
    "blog-post-comments": {
      "type": "blog-post-comments",
      "settings": {
        "show_gravatar": true
      }
    }
  },
  "order": [
    "blog-post-banner",
    "main",
    "blog-post-prev-next",
    "blog-post-comments"
  ]
}