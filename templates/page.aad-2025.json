    "experts_swiper_LaDCVP": {
      "type": "experts-swiper-new",
      "blocks": {
        "item_dhLrW3": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Dr._Adam_Rubinstein.jpg",
            "text": "\"Ulike has developed an exceptional IPL technology that empowers people to take control of their skin health with confidence, bridging the gap between clinical procedures and everyday care.\"",
            "text1": "Dr. Adam Rubinstein",
            "text2": "Award-Winning Aesthetic Surgeon"
          }
        },
        "item_tQ8rAy": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Dr._Davin_Lim_2b76d174-7961-4d4b-8cf5-3b57dd9a904b.jpg",
            "text": "“What was only possible in a clinic a few years ago has become an effective and safe option for patients to undertake at the convenience and privacy of their own home.”",
            "text1": "Dr. Davin Lim",
            "text2": "Board-Certified Dermatologist"
          }
        },
        "item_P3cp9P": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Professor_M.R._Hamblin.jpg",
            "text": "“I am to merge the most important findings in photomedicine research with Ulike’s robust research and development capabilities to attain industry breakthroughs with advanced beauty products.”",
            "text1": "Professor M.R. Hamblin",
            "text2": "Chief Scientific Advisor of Ulike Group"
          }
        }
      },
      "block_order": [
        "item_dhLrW3",
