    "add_technology_BmwhpG": {
      "type": "add-Technology",
      "blocks": {
        "item_ptWVhn": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/d1_6f354eb2-f73f-4d04-852d-f5b0355a5487.jpg",
            "image1": "shopify:\/\/shop_images\/Sapphire_Ice_Cooling_icon_ab4009f4-9169-418c-a36f-91fa8f4b7fe8.svg",
            "text": "Sapphire Ice Cooling",
            "text1": "With over a decade of research, our patented cooling system cools the treatment area to prevent burns and irritation, ensuring a smooth and comfortable hair removal experience, even in sensitive areas."
          }
        },
        "item_jjrBxy": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/d2_1ffc3b0e-131b-4ce8-999a-bcdc91e3f43a.jpg",
            "image1": "shopify:\/\/shop_images\/Dual_Lights_icon_f894cfd0-4276-433b-9851-b5895f6e51d0.svg",
            "text": "Dual Lights",
            "text1": "Ulike devices equipped with dual lamps produce 57% higher energy output and achieve a 280% faster flash rate compared to single-lamp models, resulting in enhanced efficacy and faster treatment times."
          }
        },
        "item_bjPDD4": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/d6_c44afc63-0e74-438b-8f44-5f687b2f2048.jpg",
            "image1": "shopify:\/\/shop_images\/SHR_Mode_icon_3b585f49-8ca9-401a-b720-ed334a1cb149.svg",
            "text": "SHR Mode",
            "text1": "Our Super Hair Removal mode uses dual lamps flashing alternately to progressively deliver 26J of energy per 4-pulse burst, making it exceptional in targeting and removing stubborn hair."
          }
        },
        "item_icxDLR": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/d7_ceb7b473-8b17-4915-8f93-41f85bd623a7.jpg",
            "image1": "shopify:\/\/shop_images\/Skin_Sensor_icon_07fbd12a-0d64-431f-af20-469909536289.svg",
            "text": "SkinSensor",
            "text1": "Our skin protection technology monitors your skin tone with each flash, automatically adjusting the power for safe, efficient hair removal. It even pauses flashing if the skin tone is too dark for safe treatment."
          }
        }
      },
