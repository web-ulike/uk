    "edc3fed8-df73-4b20-9c5d-fa992e8b59d0": {
      "type": "page-shave-dialog",
      "blocks": {
        "e512b357-d40e-4bc7-a93f-70f6fa079993": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/Rectangle_374_1.jpg",
