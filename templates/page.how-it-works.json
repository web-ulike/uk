    "7ae0d092-bf72-4e8d-91d3-2daca9fd1371": {
      "type": "ipl-method-comparison",
      "blocks": {
        "template--18530677489951__7ae0d092-bf72-4e8d-91d3-2daca9fd1371-16859616274cdc5733-0": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Gel_cooling.jpg",
            "title": "Gel cooling",
            "content": "<p>Uses a layer of chilled aqueous gel applied to the skin before or during the laser\/IPL treatment. It cools and protects the epidermis and reduces pain and discomfort. However, it can be messy, time-consuming and interfere with the light transmission.<\/p>"
          }
        },
        "template--18530677489951__7ae0d092-bf72-4e8d-91d3-2daca9fd1371-16859616274cdc5733-1": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Ice_pack_cooling.jpg",
            "title": "Ice pack cooling",
            "content": "<p>Uses a cold compress applied to the skin before or after the laser\/IPL treatment. It reduces pain, swelling and inflammation and is easy and inexpensive to use. However, it can be messy, cause frostbite, skin irritation and uneven cooling.<\/p>"
          }
        },
        "template--18530677489951__7ae0d092-bf72-4e8d-91d3-2daca9fd1371-16859616274cdc5733-2": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/Sapphire_Ice-cooling.jpg",
            "title": "Sapphire contact cooling",
            "content": "<p>Uses a sapphire crystal integrated into the handpiece that contacts the skin during the laser\/IPL treatment.<br\/>It cools and protects the epidermis and reduces pain and discomfort. It also allows better light transmission than metal contact cooling.<\/p>"
          }
        }
      },
